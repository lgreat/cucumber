    <bean id="cityDonorsChooseDaoFactory"
          class="gs.data.content.DonorsChooseDaoFactory" lazy-init="true"
          autowire="byName">
        <!-- cached dao properties -->
        <property name="cacheName" value="cityDonorsChooseCache"/>
        <property name="cacheSize" value="3000"/>
        <property name="cacheEntryTTLSeconds" value="86400"/>
        <property name="cacheEntryIdleSeconds" value="86400"/>
        <property name="failCacheName" value="cityDonorsChooseFailCache"/>
        <property name="failCacheSize" value="80"/>
        <property name="failCacheEntryTTLSeconds" value="60"/>
        <property name="failCacheEntryIdleSeconds" value="60"/>
        <!-- donors choose and donors choose cache properties -->
        <property name="apiKey" value="2dh64gpr"/>
        <property name="maxItemsInBucket" value="30"/>
        <property name="maxDescLength" value="45"/>
    </bean>

