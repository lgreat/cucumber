    <tag>
        <name>stateSelector</name>
        <tag-class>gs.web.state.StateSelectorTagHandler</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>useNoState</name>
            <description>Set to true if there is a "null" state option.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>noStateLabel</name>
            <description>The label to use for the first menu item, representing no or all states.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>usingLongNames</name>
            <description>Set to true if the full state names (as opposed to an abbreviation) should be
                used.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <description>CSS class to assign to the select.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>styleId</name>
            <description>CSS id to assign to the select. Defaults to "stateSelector"</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>state</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>gs.data.state.State</type>
        </attribute>
        <attribute>
            <name>name</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>multiple</name>
            <description>Whether the select be multiple or single</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <name>size</name>
            <description>For multiple selects, the size of the select box</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>stateSet</name>
            <description>For multiple selects, specify the states to pre-select</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.util.Set</type>
        </attribute>
        <attribute>
            <name>onChange</name>
            <description>onChange javascript handler</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>tabIndex</name>
            <description>field tab order</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>gradeSelector</name>
        <tag-class>gs.web.jsp.link.GradeSelectorTagHandler</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>useNoGrade</name>
            <description>Set to true if there is a "null" grade option.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>noGradeLabel</name>
            <description>The label to use for the first menu item, representing no or all grades.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <description>CSS class to assign to the select.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>styleId</name>
            <description>CSS id to assign to the select. Defaults to "gradeSelector"</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>grade</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>gs.data.school.Grade</type>
        </attribute>
        <attribute>
            <name>name</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>onChange</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>useAlternateNames</name>
            <description>Set to true to use alternate grade names. Currently just "K" for "KG".</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

