    <tag>
        <name>articlelink</name>
        <tag-class>gs.web.content.ArticleLinkTagHandler</tag-class>
        <body-content>tagdependent</body-content>
        <attribute>
            <name>article</name>
            <description>an Article object. If not provided, articleId must be
                set.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>articleId</name>
            <description>the article id, of an Article in the database. If not
                provided, article must be set.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>target</name>
            <description>generates a "target" attribute within the a tag with this
                value</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>featured</name>
            <description>if set to true, generates a link to the featured article url
                (showarticlefeature)</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>flaggedIfNew</name>
            <description>draws a "new" image for new articles</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <description>CSS class to apply to anchor tag</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>wrappingElement</name>
            <description>if set, generates a wrapping element around the link to the
                article. Useful for generating list items dynamically; if the article is
                not available, this enclosing element is NOT generated.</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>anchorName</name>
            <description>if set, adds an anchor to the link, like '#anchor'</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>defer</name>
        <tag-class>gs.web.jsp.DeferredContentTagHandler</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <name>id</name>
            <description>CSS id</description>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>form</name>
        <tag-class>gs.web.jsp.FormTagHandler</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <name>id</name>
            <description>CSS id</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>action</name>
            <description>action url</description>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>method</name>
            <description>get or post (lowercase please)</description>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>onsubmit</name>
            <description>javascript to call when the form is submitted</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>target</name>
            <description>target window to open result in _top, _parent, _blank, etc...</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <description>CSS class for form</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>adTagGenerator</name>
        <description>Generate a cobrand specific ad.
            Only ads served through GreatSchools' ad server will have body content parsed.</description>
        <tag-class>gs.web.ads.AdTagHandler</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <name>position</name>
            <description>name of ad position</description>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>articleForumLink</name>
        <description>Links to an article's forum</description>
        <tag-class>gs.web.jsp.link.ArticleForumLinkTagHandler</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <name>articleId</name>
            <description>Article id</description>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>target</name>
            <description>target window to open result in _top, _parent, _blank, etc...</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <description>CSS class for form</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>wrappingElement</name>
            <description>Element to wrap link in</description>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
</taglib>

