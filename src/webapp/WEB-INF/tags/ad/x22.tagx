<?xml version="1.0" encoding="UTF-8"?>
<jsp:root version="2.0"
          xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:gsweb="gstaglib"
          xmlns:ad="urn:jsptagdir:/WEB-INF/tags/ad"
        >
    <jsp:directive.tag body-content="empty"/>
    <jsp:directive.attribute name="position" required="false" description="Ad slot position" />
    <c:if test="${!requestScope.pageHelper.adFree and !requestScope.context.cobranded}">
        <div class="gsAd">
            <div id="adLeaderboard">
                <table cellpadding="0" cellspacing="0">
                    <tr>
                        <td>
                            <img alt="Ad" class="adSponsorImage" src="/res/img/ad/sponsor_vertical3.gif" width="20" height="96" id="jq-ghostText-Header_728x90"/>
                            <img class="hidden" src="/res/img/pixel.gif" width="20" height="96" id="jq-ghostText-Header_728x90-clear"/>
                        </td>
                        <td style="text-align:left;width:728px;height:90px;padding-right: 7px;" class="ad">
                            <ad:Header_728x90 position="${position}"/>
                        </td>
                        <td class="ad">
                            <ad:Header_212x96 />
                        </td>
                    
                    </tr>
                </table>
            </div>
        </div>
    </c:if>
</jsp:root>
