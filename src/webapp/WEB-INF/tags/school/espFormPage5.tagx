                                  </div>
                              </div>
                              <div class="lal clearfix">
                                  <c:set var="summer_program_website" value="summer_program_website_${cur_program}"/>
                                  <div class="alpha grid_5">
                                      <label for="form_${summer_program_website}">Contact: website</label>
                                  </div>
                                  <div class="grid_18 omega">
                                      <input id="form_${summer_program_website}" name="${summer_program_website}" class="js_otherField text input_6"
                                             value="${responseMap[summer_program_website].value}" placeholder="[Website url]"
                                             maxlength="100"/>
                                  </div>
                              </div>

                              <div class="lal clearfix">
                                  <c:set var="summer_program_phone" value="summer_program_phone_${cur_program}"/>
                                  <div class="alpha grid_5">
                                      <label for="form_${summer_program_phone}">Contact: phone</label>
                                  </div>
                                  <div class="grid_18 omega">
                                      <input id="form_${summer_program_phone}" name="${summer_program_phone}" class="js_otherField text input_6"
                                             value="${responseMap[summer_program_phone].value}" placeholder="[xxx-xxx-xxxx]"
                                             maxlength="14"/>
                                  </div>
                              </div>

                              <gsml:a href="" id='js_clear_${cur_program}' styleClass="js_clear">Clear this form</gsml:a>
                          </div>
                      </c:forEach>
                      <c:if test="${last_summer_program lt 5}">
                          <gsml:a href="" id="js_add_summer_program">+ add a program</gsml:a>
                      </c:if>
                  </div>
              </fieldset>
          </c:if>
        </fieldset>

