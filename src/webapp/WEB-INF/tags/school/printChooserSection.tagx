    <div id="container" class="chooser-section container_24">

        <!--<HR />-->
        <div id="header_container">

            <div style="float:left;text-align:center;">
                <span class="iconx48-orange i-48-2"><!-- do not collapse --></span>
                <p class="alignCenter topMargin">GreatSchools Rating</p>
            </div>

            <div style="float:left;padding:0 20px 0 20px;">
                <p>Academic:<br /><span class="strong">${schoolData['overallAcademicRating']}</span></p>
                <p>Climate:<br /><span class="strong">${schoolData['overallClimateRating']}</span></p>
            </div>

            <div>
                <h1>${school.name}</h1>
                <div class="header_column">
                    <p>
                        ${school.physicalAddress.street}

                        <c:if test="${not empty school.physicalAddress.streetLine2 and school.physicalAddress.streetLine2 ne ','}">
                            <br/>
                            ${school.physicalAddress.streetLine2}
                        </c:if>

                        <br/>
                        <span>${school.physicalAddress.city}</span>,
                        <span>${school.physicalAddress.state.abbreviation} </span>
                        <span>${school.physicalAddress.zip}</span>


                    </p>
                </div>
                <div class="header_column">
                    <p>
                        ${school.district.name}<br />
                        <c:if test="${not empty school.phone}">
                            Phone:<span class="strong"> ${school.phone}</span><br />
                        </c:if>

                        Class hours:<span class="strong"> 7:45 AM - 2:30 PM</span>
                    </p>
                </div>
                <div class="header_column">
                    <p>
                        Type:<span class="strong capitalize"> ${school.type.schoolTypeName}</span><br />
                        Grades:<span class="strong"> ${school.gradeLevels.rangeString}</span><br />
                        No. of students:<span class="strong"> ${school.enrollmentOrCapacity}</span>
                    </p>
                </div>
            </div>

        </div><!-- end header_container -->
        <br />
        <hr class="rulethin" />

        <div id="header_container">

            <div class="bd_column" style="width:26%; margin-right: 40px;">
                <c:if test="${not empty schoolData['ethnicityMap'] and fn:length(schoolData['ethnicityMap']) gt 0}">
                    <h2>Diversity</h2>
                    <ul>
                        <c:forEach items="${schoolData['ethnicityMap']}" var="ethnicityItem">
                            <li><span class="floatLeft">${ethnicityItem.key}</span><span class="floatRight">${ethnicityItem.value}</span>
                                <br class="clearFloat"/>
                            </li>
                        </c:forEach>
                    </ul>
                </c:if>
            </div>

            <div class="bd_column" style="width:25%; margin-right: 40px;">
                <h2>Application</h2>
                <p>
                    <c:if test="${not empty schoolData['application_deadline_date']}">
                        <span class="floatLeft">Deadline:</span><span class="floatRight strong"> <fmt:formatDate pattern="MMMMM dd, yyyy" value="${schoolData['application_deadline_date']}" /></span>
                        <br class="clearFloat"/>
                    </c:if>
                    <span class="floatLeft">Tuition:</span><span class="floatRight strong"> ${schoolData['tuition_low']} - ${schoolData['tuition_high']}</span>
                    <br class="clearFloat"/>
                    <span class="floatLeft">Financial Aid:</span><span class="floatRight strong"> ${schoolData['financial_aid']}</span>
                    <br class="clearFloat"/>
                    <span class="floatLeft">Voucher accepted:</span><span class="floatRight strong"> ${schoolData['students_vouchers']}</span>
                    <br class="clearFloat"/>
                </p>
                <h2>School Stats</h2>
                <p>
                    <span class="floatLeft">Uniforms/Dress code:</span><span class="floatRight strong"> ${schoolData['dress_code']}</span>
                    <br class="clearFloat"/>
                    <span class="floatLeft">Before care:</span><span class="floatRight strong"> Starts ${schoolData['before_after_care_start']}</span>
                    <br class="clearFloat"/>
                    <span class="floatLeft">After care:</span><span class="floatRight strong"> Ends ${schoolData['before_after_care_end']}</span>
                    <br class="clearFloat"/>
                    <span class="floatLeft">Transportation:</span><span class="floatRight strong">${schoolData['transportation']}</span>
                    <br class="clearFloat"/>
                </p>
            </div>

            <div class="bd_column" style="width:30%;">
                <h2>Special education services</h2>
                <p><span class="strong uppercase">INTENSIVE</span></p>
                <h2>ELL services</h2>
                <p><span class="strong uppercase">${schoolData['ell_level']}</span></p>
                <h2>Our grads typically go to</h2>
                <p><span class="strong uppercase">${schoolData['destination_schools']}</span></p>
            </div>
        </div>

        <!--<hr  />-->


    </div><!-- end container -->




</jsp:root>