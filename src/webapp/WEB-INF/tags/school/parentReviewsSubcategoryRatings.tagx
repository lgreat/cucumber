<?xml version="1.0" encoding="UTF-8"?>
<jsp:root version="2.0"
          xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
        >
    <jsp:directive.attribute name="ratings" required="true" description="List of NameValuePair of String,Integer"
                             type="java.util.List"/>

    <c:if test="${not empty pageScope.ratings}">
        <ul class="smallText2-777">
            <c:forEach var="rating" items="${pageScope.ratings}">
                <li class="mediaExt1-1">
                    <span class="sprite stars_s_${rating.value}_b img"
                          title="${rating.key}: ${rating.value} out of 5 stars"><!-- do not collapse --></span>
                    <div class="bd">${rating.key}</div>
                </li>
            </c:forEach>
        </ul>
    </c:if>

</jsp:root>