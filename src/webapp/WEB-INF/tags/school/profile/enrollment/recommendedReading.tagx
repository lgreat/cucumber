<?xml version="1.0" encoding="UTF-8"?>
<jsp:root version="2.0"
          xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:fn="http://java.sun.com/jsp/jstl/functions"
        >
    <c:set var="city" value="${school.city}"/>
    <div class="containerGrayBox">
        <div class="media">
            <div class="img mrl">
                <c:if test="${fn:toLowerCase(city) eq 'washington'}">
                    <img src="/res/img/content/chooserRightRail/thumbnailWashingtonDc.jpg"/>
                </c:if>
                <c:if test="${fn:toLowerCase(city) eq 'milwaukee'}">
                    <img src="/res/img/content/chooserRightRail/thumbnailMilwaukee.jpg"/>
                </c:if>
                <c:if test="${fn:toLowerCase(city) eq 'indianapolis'}">
                    <img src="/res/img/content/chooserRightRail/thumbnailIndianapolis.jpg"/>
                </c:if>
            </div>
            <p class="tal pts "><strong>Recommended reading</strong><br />
            <a href="/content/cms/translate.page?Article=${outsmartingArticle.outsmartingArticleId}"
               class="small bottom">Outsmarting the system in ${city} &amp;raquo;</a></p>
        </div>
    </div>

</jsp:root>