<?xml version="1.0" encoding="UTF-8"?>
<jsp:root version="2.0"
          xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:fn="http://java.sun.com/jsp/jstl/functions"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
        >
    <c:set var="city" value="${school.city}"/>
    <div class="containerGrayBox mbm">
        <div class="media">
            <div class="img mrl">
                <c:if test="${fn:toLowerCase(city) eq 'washington'}">
                    <gsml:img  src="/res/img/content/chooserRightRail/thumbnailWashingtonDc.jpg" width="100" height="70" alt="Washington, D.C." />
                </c:if>
                <c:if test="${fn:toLowerCase(city) eq 'milwaukee'}">
                    <gsml:img  src="/res/img/content/chooserRightRail/thumbnailMilwaukee.jpg" width="100" height="70" alt="Milwaukee" />
                </c:if>
                <c:if test="${fn:toLowerCase(city) eq 'indianapolis'}">
                    <gsml:img  src="/res/img/content/chooserRightRail/thumbnailIndianapolis.jpg" width="100" height="70" alt="Indianapolis" />
                </c:if>
            </div>
            <p class="tal pts "><strong>Recommended reading</strong><br />
            <a href="/content/cms/translate.page?Article=${outsmartingArticle.outsmartingArticleId}"
               class="small bottom">Navigating the system in ${city} &amp;raquo;</a></p>
        </div>
    </div>

</jsp:root>