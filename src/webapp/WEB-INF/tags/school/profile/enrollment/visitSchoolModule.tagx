<?xml version="1.0" encoding="UTF-8"?>
<jsp:root version="2.0"
          xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns:fn="http://java.sun.com/jsp/jstl/functions"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
        >
    <jsp:directive.attribute name="school" type="gs.data.school.School" required="true"/>


        <div class="mod standard_1-1">

                <div class="inner">
                    <div class="hd">
                        <p class="small_bold textshadow-1 pvs plm bottom">Visit</p>
                    </div>
                    <div class="bd">
                        <div class=" bg-checklist">
                            <div class="small bottom pam">
                                <h4 class="bottom">Be sure to visit</h4>
                                Take along one of<br />our checklists:<br/>
                                <c:if test="${fn:contains(school.levelCode, 'p')}">
                                    <a target="_blank" href="/catalog/pdf/schq_visit_preschool.pdf">Preschool</a><br />
                                </c:if>
                                <c:if test="${fn:contains(school.levelCode, 'e')}">
                                    <a target="_blank" href="/catalog/pdf/schq_visit_elem.pdf">Elementary school</a><br />
                                </c:if>
                                <c:if test="${fn:contains(school.levelCode, 'm')}">
                                    <a target="_blank" href="/catalog/pdf/schq_visit_middle.pdf">Middle school</a><br />
                                </c:if>
                                <c:if test="${fn:contains(school.levelCode, 'h')}">
                                    <a target="_blank" href="/catalog/pdf/schq_visit_high.pdf">High school</a><br />
                                </c:if>
                                <br />
                            </div>
                        </div>
                        <hr class="keyline2"/>
                        <div class="mam">
                            <h4>Finding the right school</h4>
                            <p>
                                <c:if test="${fn:contains(school.levelCode, 'e')}">
                                    <a href="/school-choice/school-choice/7055-choose-elementary-school-video.gs">
                                        <div class="tac smaller">
                                            <img src="/res/img/content/video/schoolTourThumb-e.png" alt="Touring a school video thumbnail"/><br />
                                            What to look for in an<br/>elementary school
                                        </div>
                                    </a>
                                </c:if>
                            </p>
                            <p>
                                <c:if test="${fn:contains(school.levelCode, 'm')}">
                                    <a href="/school-choice/school-choice/7056-choose-middle-school-video.gs">
                                        <div class="tac smaller">
                                            <img src="/res/img/content/video/schoolTourThumb-m.png" alt="Touring a school video thumbnail"/><br />
                                            What to look for in a<br/>middle school
                                        </div>
                                    </a>
                                </c:if>
                            </p>
                            <p>
                                <c:if test="${fn:contains(school.levelCode, 'h')}">
                                    <a href="/school-choice/school-choice/7066-choose-high-school-video.gs">
                                        <div class="tac smaller">
                                            <img src="/res/img/content/video/schoolTourThumb-h.png"/><br />
                                            What to look for in a<br/>high school
                                        </div>
                                    </a>
                                </c:if>
                            </p>

                        </div>
                    </div>

            </div>
        </div>

</jsp:root>