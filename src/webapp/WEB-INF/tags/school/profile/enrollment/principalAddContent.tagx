<?xml version="1.0" encoding="UTF-8"?>
<jsp:root version="2.0"
          xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns:link="urn:www.greatschools.org/taglib/link"
        >
    <jsp:directive.attribute name="school" type="gs.data.school.School" required="true"/>

    <link:schoolEspRegister styleClass="no_interrupt tdn" school="${school}">
        <div class="mod standard_3-1">
            <div class="outer">
                <div class="inner bg-principal ie-moduleHeight-162">
                    <div class="hd">
                        <h3 class="bottom">Are you responsible for enrollment at your school?</h3>
                    </div>
                    <div class="bd" style="height:78px;">
                        <p class="small mtm bottom">Tell parents<br />
                            more about<br />
                            your school
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </link:schoolEspRegister>
</jsp:root>