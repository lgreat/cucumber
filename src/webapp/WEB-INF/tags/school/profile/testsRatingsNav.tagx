<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml" version="2.0"
          xmlns:gsweb="gstaglib"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:profile="urn:jsptagdir:/WEB-INF/tags/school/profile"
        >

    <jsp:directive.tag body-content="empty"/>

    <jsp:directive.attribute name="schoolName" required="true" type="java.lang.String"
                             description="Name of the school"/>
    <jsp:directive.attribute name="showTestScores" type="java.lang.Boolean" description="False to hide the test scores nav"/>
    <jsp:directive.attribute name="showRatingsDetails" type="java.lang.Boolean" description="False to hide the ratings details nav"/>
    <jsp:directive.attribute name="showCollegeReadiness" type="java.lang.Boolean" description="False to hide the college readiness nav"/>

    <ul class="tabStyle-5">
        <c:if test="${empty showTestScores or showTestScores eq true}">
            <profile:gsTab tabName="test-scores" title="Test Scores" tabText="Test scores" cssClass="${requestScope.schoolProfileTabMap['test-scores']}" />
        </c:if>
        <c:if test="${empty showRatingsDetails or showRatingsDetails eq true}">
            <profile:gsTab tabName="ratings" title="School Ratings" tabText="Ratings details" cssClass="${requestScope.schoolProfileTabMap.ratings}" addDivider="${empty showTestScores or showTestScores eq true}" />
        </c:if>
        <c:if test="${empty showCollegeReadiness or showCollegeReadiness eq true}">
            <profile:gsTab tabName="college-readiness" title="College preparedness" tabText="College preparedness" cssClass="${requestScope.schoolProfileTabMap['college-readiness']}" addDivider="${empty showTestScores or showTestScores eq true or empty showRatingsDetails or showRatingsDetails eq true}" />
        </c:if>
    </ul>
</jsp:root>