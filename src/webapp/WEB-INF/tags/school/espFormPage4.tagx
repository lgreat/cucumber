<fieldset>

<!--Only show for schools with level 'h'-->
<c:if test="${not empty school.levelCode and (fn:indexOf(school.levelCode,'h') >=0) }">
    <legend>College and career</legend>
    <hr class="legend"><!--Do not collapse--></hr>
</c:if>

<c:if test="${not empty school.levelCode and (fn:indexOf(school.levelCode,'h') >=0) }">
    <div class="lal clearfix">
        <div class="alpha grid_7">
            <p class="labelGroup">What college preparation/awareness resources does your school provide to students?</p>
        </div>

