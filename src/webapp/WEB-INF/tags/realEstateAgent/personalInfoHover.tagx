<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:gsweb="gstaglib"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:form="http://www.springframework.org/tags/form"
          version="2.0">

    <div class ="jq-personalInfoForm">
        <div class="hd"><!--yellow header-->
            1 Contact Info
        </div>
        <div class="bd" style="height: 300px;">
            <div class="tac">
                <h2>Contact information</h2>
            </div>
            <div class="ptl">
                <div class="clearfix">
                    <div class="fl">
                        <gsml:input type="text" styleId="jq-fName" styleClass="text1 input_5" name="firstName"
                                    placeholder="First Name" value="First Name"/>
                    </div>
                    <div class="fr">
                        <gsml:input type="text" styleId="jq-lName" styleClass="text1 input_5" name="lastName"
                                    placeholder="Last Name" value="Last Name"/>
                    </div>
                </div>
                <div class="mtl">
                    <gsml:input type="text" styleId="jq-email" styleClass="text1 input_10" name="email"
                                value="Email" placeholder="Email"/>
                </div>
                <div class="mtl">
                    <gsml:input type="password" styleId="jq-password" styleClass="text1 input_10" name="password"
                                value="Password" placeholder="Password"/>
                </div>
            </div>
        </div>

        <div class="ft joinSubmit">
            <div class="tac">
                <a href="javascript:void(0)">
                    <button class="button-1 jq-personalInfoSubmit" name="personalInfoSubmit">
                        Complete
                    </button>
                </a>
            </div>
        </div>
    </div>
</jsp:root>