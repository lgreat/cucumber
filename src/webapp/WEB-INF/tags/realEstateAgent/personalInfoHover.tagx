<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:gsweb="gstaglib"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:form="http://www.springframework.org/tags/form"
          version="2.0">

    <div class ="jq-personalInfoForm">
        <div class="hd">
            <span class="real-estate-step-1 re-contact-selected"><!-- do not collapse --></span>
            <span class="real-estate-step-2 re-company-info-default"><!-- do not collapse --></span>
            <span class="real-estate-step-3 re-photo-upload-default"><!-- do not collapse --></span>
        </div>
        <div class="bd" style="height: 330px;">
            <div class="tac">
                <h2>Contact information</h2>
            </div>
            <div class="ptl">
                <div class="clearfix">
                    <div class="fl jq-firstNameFields">
                        <gsml:input type="text" styleId="jq-fName" styleClass="text1 input_5" name="firstName"
                            value="First Name" onkeypress="GS_textSwitch(this, '', 'First Name');" onblur="GS_textSwitch(this, 'First Name', '');"/>
                        <div class="errors">
                            <span class="error valid">&amp;nbsp;</span>
                            <span class="error invalid"><!-- not empty --></span>
                        </div>
                    </div>
                    <div class="fr jq-lastNameFields">
                        <gsml:input type="text" styleId="jq-lName" styleClass="text1 input_5" name="lastName"
                            value="Last Name" onkeypress="GS_textSwitch(this, '', 'Last Name');" onblur="GS_textSwitch(this, 'Last Name', '');"/>
                        <div class="errors">
                            <span class="error valid">&amp;nbsp;</span>
                            <span class="error invalid"><!-- not empty --></span>
                        </div>
                    </div>
                </div>
                <div class="mtm jq-emailFields">
                    <gsml:input type="text" styleId="jq-email" styleClass="text1 input_10" name="email"
                        value="Work Email" onkeypress="GS_textSwitch(this, '', 'Work Email');" onblur="GS_textSwitch(this, 'Work Email', '');"/>
                    <div class="errors">
                        <span class="error valid">&amp;nbsp;</span>
                        <span class="error invalid"><!-- not empty --></span>
                    </div>
                </div>
                <div class="mtm jq-passwordFields">
                    <gsml:input type="text" styleId="jq-password" styleClass="text1 input_10" name="password"
                        value="Password" onfocus="GS_textSwitch(this, '', 'Password'),(this.type='password');"
                        onblur="GS_textSwitch(this, 'Password', '');"/>
                    <div class="errors">
                        <span class="error valid">&amp;nbsp;</span>
                        <span class="error invalid"><!-- not empty --></span>
                    </div>
                </div>
            </div>
            <div class="small">By joining GreatSchools, you agree to our GreatSchools <a href="/terms/" target="_blank">Terms of Use</a> and <a href="/terms/#communityStandards" target="_blank">Community Guidelines</a>.</div>
        </div>
        <div class="ft joinSubmit">
            <div class="tac">
                <a href="javascript:void(0)">
                    <button class="but but-lg but-1 jq-personalInfoSubmit" name="personalInfoSubmit">
                        Next step
                    </button>
                </a>
            </div>
        </div>
    </div>
</jsp:root>
