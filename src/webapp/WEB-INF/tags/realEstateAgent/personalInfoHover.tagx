<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:gsweb="gstaglib"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:form="http://www.springframework.org/tags/form"
          version="2.0">

    <div class ="jq-personalInfoForm">
        <div class="hd">
            <span class="real-estate-step-1 re-contact-selected"><!-- do not collapse --></span>
            <span class="real-estate-step-2 re-company-info-default"><!-- do not collapse --></span>
            <span class="real-estate-step-3 re-photo-upload-default"><!-- do not collapse --></span>
        </div>
        <div class="bd" style="height: 330px;">
            <div class="tac">
                <h2>Contact information</h2>
            </div>
            <div class="ptl">
                <div class="clearfix">
                    <div class="fl">
                        <gsml:input type="text" styleId="jq-fName" styleClass="text1 input_5" name="firstName"
                                    placeholder="First Name" value="First Name" onfocus="GS_textSwitch(this, '', 'First Name');" onblur="GS_textSwitch(this, 'First Name', '');"/>
                    </div>
                    <div class="fr">
                        <gsml:input type="text" styleId="jq-lName" styleClass="text1 input_5" name="lastName"
                                    placeholder="Last Name" value="Last Name" onfocus="GS_textSwitch(this, '', 'Last Name');" onblur="GS_textSwitch(this, 'Last Name', '');"/>
                    </div>
                </div>
                <div class="mtl">
                    <gsml:input type="text" styleId="jq-email" styleClass="text1 input_10" name="email"
                                placeholder="Email" value="Email" onfocus="GS_textSwitch(this, '', 'Email');" onblur="GS_textSwitch(this, 'Email', '');"/>
                </div>
                <div class="mtl">
                    <gsml:input type="password" styleId="jq-password" styleClass="text1 input_10" name="password"
                                placeholder="Password" value="Password" onfocus="GS_textSwitch(this, '', 'Password');" onblur="GS_textSwitch(this, 'Password', '');"/>
                </div>
            </div>
        </div>

        <div class="ft joinSubmit">
            <div class="tac">
                <a href="javascript:void(0)">
                    <button class="but but-lg but-1 jq-personalInfoSubmit" name="personalInfoSubmit">
                        Next step
                    </button>
                </a>
            </div>
        </div>
    </div>
</jsp:root>