<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:gsweb="gstaglib"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:form="http://www.springframework.org/tags/form"
          version="2.0">

    <div class="jq-businessInfoForm dn">
        <div class="hd"><!--yellow header-->
            2 Company Info
        </div>
        <div class="bd" style="height: 300px;">
            <div class="tac">
                <h2>Company information</h2>
            </div>
            <div class="ptl">
                <div class="">
                    <gsml:input type="text" styleId="jq-companyName" styleClass="text1 input_10" name="companyName"
                                value="Company Name" placeholder="Company Name"/>
                </div>
                <div class="mtl clearfix">
                    <div class="fl">
                        <gsml:input type="text" styleId="jq-workNumber" styleClass="text1 input_5" name="workNumber"
                                    value="Work #"/>
                    </div>
                    <div class="fr">
                        <gsml:input type="text" styleId="jq-cellNumber" styleClass="text1 input_5" name="cellNumber"
                                    value="Cell #" placeholder="Cell #"/>
                    </div>
                </div>
                <div class="mtl">
                    <gsml:input type="text" styleId="jq-address" styleClass="text1 input_10" name="address"
                                value="Address" placeholder="Address"/>
                </div>
                <div class="mtl clearfix">
                    <div class="fl">
                        <gsml:input type="text" styleId="jq-city" styleClass="text1 input_3" name="city" value="City"
                                    placeholder="City"/>
                    </div>
                    <div class="mhl fl">
                        <gsweb:stateSelector useNoState="true" noStateLabel="State"
                                             usingLongNames="false"
                                             styleClass="stateSelect"
                                             styleId="jq-state" name="state"/>
                    </div>
                    <div class="fr">
                        <gsml:input type="text" styleId="jq-zip" styleClass="text1 input_3" name="zip"
                                    placeholder="Zip"/>
                    </div>
                </div>
            </div>
        </div>
        <div class="ft joinSubmit">
            <div class="tac">
                <a href="javascript:void(0)">
                    <button class="button-1 jq-businessInfoSubmit" name="businessInfoSubmit">
                        Complete
                    </button>
                </a>
            </div>
        </div>
    </div>

</jsp:root>