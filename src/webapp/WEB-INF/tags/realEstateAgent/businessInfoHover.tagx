<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:gsweb="gstaglib"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:form="http://www.springframework.org/tags/form"
          version="2.0">

    <div class="jq-businessInfoForm dn">

        <div class="inner">
            <div class="hoverTitle hd">
                2 Company Info
            </div>
        </div>

        <div class="bd tal">
            <div class="introTxt col">
                <p class="small_bold title">Company information</p>
            </div>

            <div class="line">
                <div class="unit size1of1 lastUnit">
                    <div class="line lal">
                        <gsml:input type="text" styleId="jq-companyName" styleClass="text input_15" name="companyName" placeholder="Company name"/>
                        <div class="spacer_60"><!-- do not collapse --></div>
                    </div>

                    <div class="line lal">
                        <div class="unit fl">
                            <gsml:input type="text" styleId="jq-workNumber" styleClass="text input_7" name="workNumber" placeholder="Work#"/>
                        </div>
                        <div class="unit lastUnit fr">
                            <gsml:input type="text" styleId="jq-cellNumber" styleClass="text input_7" name="cellNumber" placeholder="Cell#"/>
                        </div>
                    </div>

                    <div class="line lal">
                        <gsml:input type="text" styleId="jq-address" styleClass="text input_15" name="address" placeholder="Address"/>
                    </div>

                    <div class="line lal">
                        <div class="unit fl">
                            <gsml:input type="text" styleId="jq-city" styleClass="text input_4" name="city"
                                        placeholder="City"/>
                        </div>

                        <div class="unit">

                            <gsweb:stateSelector useNoState="true" noStateLabel="State"
                                                 usingLongNames="false"
                                                 styleClass="stateSelect"
                                                 styleId="jq-state" name="state"/>
                        </div>

                        <div class="unit lastUnit fr">
                            <gsml:input type="text" styleId="jq-zip" styleClass="text input_4" name="zip"
                                        placeholder="Zip"/>
                        </div>
                    </div>

                </div>
            </div>

            <div class="line joinSubmit lal">
                <div class="">
                    <a href="javascript:void(0)">
                        <button class="button-1 jq-businessInfoSubmit" name="businessInfoSubmit">
                            Complete
                        </button>
                    </a>
                </div>
            </div>
        </div>
    </div>

</jsp:root>