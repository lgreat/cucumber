<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:gsweb="gstaglib"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:form="http://www.springframework.org/tags/form"
          version="2.0">

    <div class="jq-businessInfoForm dn">
        <div class="hd"><!--yellow header-->
            <span class="real-estate-step-1 re-contact-default"><!-- do not collapse --></span>
            <span class="real-estate-step-2 re-company-info-selected"><!-- do not collapse --></span>
            <span class="real-estate-step-3 re-photo-upload-default"><!-- do not collapse --></span>
        </div>
        <div class="bd" style="height: 330px;">
            <div class="tac">
                <h2>Company information</h2>
            </div>
            <div class="ptl">
                <div class="jq-companyNameFields">
                    <gsml:input type="text" styleId="jq-companyName" styleClass="text1 input_10" name="companyName"
                                value="${(not empty companyName) ? companyName : 'Company Name'}" onfocus="GS_textSwitch(this, '', 'Company Name');" onblur="GS_textSwitch(this, 'Company Name', '');"/>

                    <div class="errors">
                        <span class="error valid">&amp;nbsp;</span>
                        <span class="error invalid"><!-- not empty --></span>
                    </div>
                </div>
                <div class="mtl clearfix">
                    <div class="fl jq-workNumberFields">
                        <gsml:input type="text" styleId="jq-workNumber" styleClass="text1 input_5" name="workNumber"
                                    value="${(not empty workNumber) ? workNumber : 'Work #'}" onfocus="GS_textSwitch(this, '', 'Work #');" onblur="GS_textSwitch(this, 'Work #', '');"/>

                        <div class="errors">
                            <span class="error valid">&amp;nbsp;</span>
                            <span class="error invalid"><!-- not empty --></span>
                        </div>
                    </div>
                    <div class="fr">
                        <gsml:input type="text" styleId="jq-cellNumber" styleClass="text1 input_5" name="cellNumber"
                                    value="${(not empty cellNumber) ? cellNumber : 'Cell #'}"
                                    onfocus="GS_textSwitch(this, '', 'Cell #');" onblur="GS_textSwitch(this, 'Cell #', '');"/>
                    </div>
                </div>
                <div class="mtl jq-addressFields">
                    <gsml:input type="text" styleId="jq-address" styleClass="text1 input_10" name="address"
                                value="${(not empty address) ? address : 'Work Address'}"
                                onfocus="GS_textSwitch(this, '', 'Work Address');" onblur="GS_textSwitch(this, 'Work Address', '');"/>

                    <div class="errors">
                        <span class="error valid">&amp;nbsp;</span>
                        <span class="error invalid"><!-- not empty --></span>
                    </div>

                </div>
                <div class="mtl clearfix">
                    <div class="fl jq-cityFields">
                        <gsml:input type="text" styleId="jq-city" styleClass="text1 input_3" name="city"
                                    value="${(not empty city) ? city : 'City'}"
                                    onfocus="GS_textSwitch(this, '', 'City');" onblur="GS_textSwitch(this, 'City', '');"/>

                        <div class="errors">
                            <span class="error valid">&amp;nbsp;</span>
                            <span class="error invalid"><!-- not empty --></span>
                        </div>
                    </div>
                    <div class="mhl fl jq-stateFields">
                        <!--<gsweb:stateSelector useNoState="${empty state}" noStateLabel="State"-->
                                             <!--usingLongNames="false"-->
                                             <!--styleClass="stateSelect"-->
                                             <!--styleId="" name="state" state="${state}"/>-->

                        <div id="jq-state" class="pr js-realEstateState">
                            <div class="js-selectBox selectBox2 selectList clearfix">
                                <div class="js-selectBoxText mrs fl">${not empty state ? state : "State"}</div>
                                <div class="fr"><span
                                        class="vam iconx16 i-16-sort-list-arrow-down"><!--do not collapse--></span>
                                </div>
                            </div>
                            <div class="js-selectDropDown selectDropDown2 pa phm ptm dn">
                                <c:forEach var="stateObj" items="${stateList}">
                                    <div class="js-ddValues">${stateObj.abbreviation}</div>
                                </c:forEach>
                            </div>
                        </div>

                        <div class="errors">
                            <span class="error valid">&amp;nbsp;</span>
                            <span class="error invalid"><!-- not empty --></span>
                        </div>
                    </div>
                    <div class="fr jq-zipFields">
                        <gsml:input type="text" styleId="jq-zip" styleClass="text1 input_3" name="zip"
                                    value="${(not empty zip) ? zip : 'Zip'}"
                                    onfocus="GS_textSwitch(this, '', 'Zip');" onblur="GS_textSwitch(this, 'Zip', '');"/>

                        <div class="errors">
                            <span class="error valid">&amp;nbsp;</span>
                            <span class="error invalid"><!-- not empty --></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="ft joinSubmit">
            <div class="tac">
                <a href="javascript:void(0)">
                    <button class="but but-lg but-1 jq-businessInfoSubmit" name="businessInfoSubmit">
                        Next step
                    </button>
                </a>
            </div>
        </div>
    </div>

</jsp:root>
