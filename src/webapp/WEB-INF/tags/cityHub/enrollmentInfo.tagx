<?xml version="1.0" encoding="UTF-8"?>
<jsp:root version="2.0"
          xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsweb="gstaglib"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:hub="urn:jsptagdir:/WEB-INF/tags/cityHub"
>

    <!-- Carasole 1 -->

    <div class="fl mrl" style="margin-top: 50px">
        <a href="#" id="prev1">
            <span class="iconx32g i-32-carouselLeft"><!-- do not collapse --></span>
        </a>
    </div>
    <div id="artifact_slide" class="artifact_slide"
         data-cycle-fx="scrollHorz"
         data-cycle-slides="> div"
         data-cycle-timeout="0"
         data-cycle-easing="linear"
         data-cycle-next="#next1"
         data-cycle-prev="#prev1">

        <div>
            <gsml:img src="/res/img/cityHubs/partner1.png" alt="Partner 1" styleClass="mrm"  style="border-right: 1px solid #cccccc"/>

        </div>
        <div>
            <gsml:img src="/res/img/cityHubs/partner2.png" alt="Partner 2" styleClass="mrm"  style="border-right: 1px solid #cccccc"/>

        </div>
        <div >
            <gsml:img src="/res/img/cityHubs/partner3.png"
                      alt="Partner 3" styleClass="mrm"  style="border-right: 1px solid #cccccc"/>

        </div>

    </div>


    <div class="fl" style="margin-top: 50px; margin-left: 25px;">
        <a href="#" id="next1">
            <span class="iconx32g i-32-carouselRight vam"><!-- do not collapse --></span>
        </a>
    </div>




    <div class="fl mrl" style="margin-top: 50px">
        <a href="#" id="prev2">
            <span class="iconx32g i-32-carouselLeft"><!-- do not collapse --></span>
        </a>
    </div>

    <!-- Carasole 2 -->

    <div id="artifact_slide1" class="artifact_slide1"
         data-cycle-fx="scrollHorz"
         data-cycle-slides="> div"
         data-cycle-timeout="0"
         data-cycle-easing="linear"
         data-cycle-next="#next2"
         data-cycle-prev="#prev2">

        <div >
            123   Hello I am testing

        </div>
        <div >
            456 Hello I am testing</div>
        <div >
            678  Hello I am testing
        </div>
    </div>


    <div class="fl" style="margin-top: 50px; margin-left: 25px;">
        <a href="#" id="next2">
            <span class="iconx32g i-32-carouselRight vam"><!-- do not collapse --></span>
        </a>
    </div>

    <div class="tab-content-container">
        <c:forEach items="${enrollmentsInfo}" var="enrollmentInfo">
            <c:choose>
                <c:when test="${defaultTab == enrollmentInfo.tabName}">
                    <div class="container_24 clearfix mvl" data-gs-tab-content="${enrollmentInfo.tabName}"
                         data-gs-tab-group="groupOne" data-gs-tab-inactive="inactive-tab-content">
                        <div class="alpha grid_12 suffix_1">
                            <h3 class="SourceSansPro-Semibold-h3">
                                ${gsweb:capitalize(enrollmentInfo.schoolType.schoolTypeName)} ${enrollmentInfo.tabName}
                            </h3>
                            <p>${enrollmentInfo.description}</p>
                            <h5 class="SourceSansPro-Semibold-h5">For more information:</h5>
                            <c:if test="${not empty enrollmentInfo.moreInfo.address}">
                                ${enrollmentInfo.moreInfo.address}<br/>
                            </c:if>
                            <c:if test="${not empty enrollmentInfo.moreInfo.link}">
                                <c:forEach items="${enrollmentInfo.moreInfo.link}" var="link">
                                    <a href="${link.hrefXml}" target="_blank">${link.contents}</a><br/>
                                </c:forEach>
                            </c:if>
                        </div>
                        <div class="grid_11 omega">
                            <hub:enrollmentKeyDates
                                    schoolType="${gsweb:toLowerCase(enrollmentInfo.schoolType.schoolTypeName)}"
                                    gradeLevel="${gsweb:toLowerCase(enrollmentInfo.levelCode.lowestLevel.longName)}"/>
                            <c:forEach items="${enrollmentInfo.tipsInfoModel}" var="tip">
                                <div class="mod standard_2-1 mtl">
                                    <div class="inner">
                                        <div class="pal tac">
                                            <h6 class="SourceSansPro-Semibold-h6">Did you know?</h6>
                                            <span class="small">${tip}</span>
                                        </div>
                                    </div>
                                </div>
                            </c:forEach>
                            <button class="but-4 clearfix" style="width: auto;" type="button"
                                    onclick="location.href='${enrollmentInfo.browseLink.hrefXml}'">
                                <span class="fl">${enrollmentInfo.browseLink.contents}</span>
                                <span class="fr plm mlm" style="border-left: 1px solid #b8b8b8">${enrollmentInfo.browseLink.count}</span>
                            </button>
                        </div>
                    </div>                          
                </c:when>
                <c:otherwise>
                    <div class="container_24 clearfix mvl dn" data-gs-tab-content="${enrollmentInfo.tabName}"
                         data-gs-tab-group="groupOne" data-gs-tab-inactive="dn">
                        <div class="alpha grid_12 suffix_1">
                            <h3 class="SourceSansPro-Semibold-h3">
                                ${gsweb:capitalize(enrollmentInfo.schoolType.schoolTypeName)} ${enrollmentInfo.tabName}
                            </h3>
                            <p>${enrollmentInfo.description}</p>
                            <h5 class="SourceSansPro-Semibold-h5">For more information:</h5>
                            <c:if test="${not empty enrollmentInfo.moreInfo.address}">
                                ${enrollmentInfo.moreInfo.address}<br/>
                            </c:if>
                            <c:if test="${not empty enrollmentInfo.moreInfo.link}">
                                <c:forEach items="${enrollmentInfo.moreInfo.link}" var="link">
                                    <a href="${link.hrefXml}" target="_blank">${link.contents}</a><br/>
                                </c:forEach>
                            </c:if>
                        </div>
                        <div class="grid_11 omega">
                            <hub:enrollmentKeyDates
                                    schoolType="${gsweb:toLowerCase(enrollmentInfo.schoolType.schoolTypeName)}"
                                    gradeLevel="${gsweb:toLowerCase(enrollmentInfo.levelCode.lowestLevel.longName)}"/>
                            <c:forEach items="${enrollmentInfo.tipsInfoModel}" var="tip">
                                <div class="mod standard_2-1 mtl">
                                    <div class="inner">
                                        <div class="pal tac">
                                            <h6 class="SourceSansPro-Semibold-h6">Did you know?</h6>
                                            <span class="small">${tip}</span>
                                        </div>
                                    </div>
                                </div>
                            </c:forEach>
                            <button class="but-4 clearfix" type="button" style="width: auto;"
                                    onclick="location.href='${enrollmentInfo.browseLink.hrefXml}'">
                                <span class="fl">${enrollmentInfo.browseLink.contents}</span>
                                <span class="fr plm mlm" style="border-left: 1px solid #b8b8b8">${enrollmentInfo.browseLink.count}</span>
                            </button>
                        </div>
                    </div>                           
                </c:otherwise>
            </c:choose>
        </c:forEach>
    </div>

</jsp:root>
