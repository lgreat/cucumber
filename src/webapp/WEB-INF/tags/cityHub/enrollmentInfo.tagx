<?xml version="1.0" encoding="UTF-8"?>
<jsp:root version="2.0"
          xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsweb="gstaglib"
          xmlns:hub="urn:jsptagdir:/WEB-INF/tags/cityHub"
>

    <c:forEach items="${enrollmentsInfo}" var="enrollmentInfo">
        TAB NAME : ${enrollmentInfo.tabName} <br/>
        HEADING: ${enrollmentInfo.schoolTypeDescription} <br/>
        DESCRIPTION: ${enrollmentInfo.description} <br/> <br/>

        <c:forEach items="${enrollmentInfo.tipsInfoModel}" var="tip" >
            TIP TEXT "${tip}"<br/><br/>
        </c:forEach>
        <c:if test="${not empty enrollmentInfo.moreInfo.address}">
            MORE INFO  ADDRESS ${enrollmentInfo.moreInfo.address}<br/>
        </c:if>

        <c:if test="${not empty enrollmentInfo.moreInfo.link}">
            <c:forEach items="${enrollmentInfo.moreInfo.link}" var="link">
                MORE INFO  LINK  <a href="${link.hrefXml}" target="_blank"
                   >${link.contents}</a><br/>

            </c:forEach>
        </c:if>

         <hub:enrollmentKeyDates schoolType="${gsweb:toLowerCase(enrollmentInfo.schoolType.schoolTypeName)}"
                                 gradeLevel="${gsweb:toLowerCase(enrollmentInfo.levelCode.lowestLevel.longName)}"/>
        <button class="but-4 clearfix" style="width: auto" type="button" onclick="location.href='${enrollmentInfo.browseLink.hrefXml}'">
            <span class="fl">${enrollmentInfo.browseLink.contents}</span>
            <span class="fr plm" style="border-left: 1px solid #b8b8b8">${enrollmentInfo.browseLink.count}</span>
        </button>                            
     </c:forEach>
</jsp:root>
