<?xml version="1.0" encoding="UTF-8"?>
<jsp:root version="2.0"
          xmlns:jsp="http://java.sun.com/JSP/Page"
        >


    <div id="hover_forgotPassword" class="dn">
    <div class="mod modal_1 width_13 " id="hover_forgotPassword" >
    <div class="outer">
        <div class="fr"><span class="iconx24 i-24-close  js_closeHover"><!-- do not collapse --></span></div>
    <div class="inner">
    <div class="hoverTitle hd">
        <h2 class="fl">Forgot your password?</h2>

        <div class="clearFloat" style="clear: both"><!-- do not collapse --></div>
    </div>

        <form id="hover_forgotPasswordForm" action="#">
            <div class="media error messages">
            <span class="iconx16 i-16-alert img mrs"><!-- do not collapse --></span>
            <div class="bd">

                <!-- not empty -->
            </div>
            </div>
            <fieldset class="hoverFormFieldset">
                <p>Please enter the email address associated with your account. Instructions for selecting a new password will be emailed to you.<br/>
                </p>
                <label for="fpemail" class="prm">Email address</label><input type="text" class="text-smaller input_5" id="fpemail" name="email"/>
            </fieldset>
            <fieldset class="btns ptl">

                <button class="button-1 mrl" type="submit" name="forgotPasswordSubmit" id="hover_forgotPasswordSubmit">Reset password</button>

                <button class="button-1 js_closeHover" type="button" name="forgotPasswordCancel">Cancel</button>
            </fieldset>
        </form>
    </div>
    </div></div></div>
</jsp:root>