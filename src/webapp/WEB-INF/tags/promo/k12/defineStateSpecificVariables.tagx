            <c:set var="tuitionText" value="Online private school" scope="request"/> <!-- Defaults to this text. INT overrides -->
            <c:set var="gradesServed" value="K-12" scope="request"/>
            <!-- TODO: Set rest of variables -->
        </c:otherwise>
    </c:choose>

</jsp:root>