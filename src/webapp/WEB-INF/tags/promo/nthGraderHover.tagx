<?xml version="1.0" encoding="UTF-8"?>
<jsp:root version="2.0"
          xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
        >
    <jsp:directive.tag body-content="scriptless"/>
    <jsp:directive.attribute name="referredBy" required="true" description="Omniture page name of page that this hover appears on. Unused!"/>
    <jsp:directive.attribute name="autocheck" required="false" description="Optional pre-checked newsletter"/>
    <jsp:directive.attribute name="isForm" required="false" description="Unused. Don't bother setting this!!"/>
    <jsp:directive.attribute name="styleClass" required="false" description="Class name for anchor link, only provide if you do not want a form."/>

    <pageHelper:showNthGraderHover/>

    <gsml:a href="/email/management.page"
            onClick="return nthGraderRegistrationDialog.prepAndShow(this.href, '${autocheck}');" styleClass="${styleClass}">
     <jsp:doBody/>
    </gsml:a>
</jsp:root>