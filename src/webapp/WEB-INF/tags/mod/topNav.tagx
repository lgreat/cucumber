<?xml version="1.0" encoding="UTF-8"?>
<jsp:root version="2.0"
          xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:ad="urn:jsptagdir:/WEB-INF/tags/ad"
          xmlns:mod="urn:jsptagdir:/WEB-INF/tags/mod"
          xmlns:link="urn:www.greatschools.net/taglib/link"
          xmlns:gsweb="gstaglib"
        >
<jsp:directive.tag body-content="empty"/>
<ad:x22/>

<div id="topnav">
<table border="0" cellpadding="0" cellspacing="0" style="width: 100%">
<tr>
<td><gsml:img src="/res/img/pixel.gif" alt="" style="height:102px; width: 18px"/></td>
<td  style="width:401px;height:102px">
    <c:choose>
        <c:when test="${requestScope.pageHelper.logoLinked}">
            <gsml:a href="/"><gsml:img src="/res/img/logo/logo_GS_330x110.gif" alt="GreatSchools.net logo"
                                       /></gsml:a>
        </c:when>
        <c:otherwise>
            <gsml:img src="/res/img/logo/logo_GS_330x110.gif" alt="GreatSchools.net logo"
                      style="width:401px;height:102px"/>
        </c:otherwise>
    </c:choose>
</td>
<td><gsml:img src="/res/img/pixel.gif" alt="" style="height:102px"/></td>
<td>
    <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
            <td><gsml:img src="/res/img/pixel.gif" alt=""/></td>
            <td>
                <table width="100%" border="0" cellspacing="0" cellpadding="0" class="member">
                    <tr>
                        <c:if test="${requestScope.context.user==null}">
                            <td class="welcome">Welcome!</td>
                            <td class="signout"><gsml:a href="/community/login.page" >Sign
                                in</gsml:a></td>
                            <td class="saved"><gsml:a href="/cgi-bin/msl_confirm/$STATE">Start saving schools</gsml:a>
                            </td>
                        </c:if>
                        <c:if test="${requestScope.context.user!=null}">
                            <td class="welcome">Welcome ${requestScope.context.user.email}</td>
                            <td class="signout"><gsml:a
                                    href="/cgi-bin/logout/$STATE/?email=${requestScope.context.user.email}&amp;mid=${requestScope.context.user.id}">
                                Sign
                                out</gsml:a></td>
                            <td class="saved"><gsml:a href="/cgi-bin/msl_confirm/$STATE">32 Saved schools</gsml:a></td>
                        </c:if>
                    </tr>
                </table>
            </td>
            <td>&#160;</td>
        </tr>
        <tr>
            <td colspan="3"><gsml:img src="/res/img/pixel.gif" alt="" style="width:29px;height:12px"/></td>
        </tr>
        <tr>
            <td>&#160;</td>
            <td>
                <gsml:form method="get" action="/stateLauncher.page">
                    <input name="p" value="search_single" type="hidden"/>
                    <c:if test="${requestScope.context.cobrand == 'charterschoolratings'}">
                        <input type="hidden" name="st" value="charter"/>
                    </c:if>
                    <input name="p" value="search_single" type="hidden"/>
                    <table style="width:301px" border="0" cellpadding="0" cellspacing="0">
                        <tr>
                            <td style="width:202px">
                                <span class="searchLabel" id="searchPrompt">Enter school, district or city</span>
                            </td>
                            <td style="width:10px">
                                <gsml:img src="/res/img/pixel.gif" alt="" style="width:6px;height:6px"/>
                            </td>
                            <td style="width:50px">
                                <span class="searchLabel">Search in:</span>
                            </td>
                            <td style="width:43px;vertical-align: middle">
                                <gsml:img src="/res/img/pixel.gif" alt="" style="width:6px;height:6px"/>
                            </td>
                        </tr>
                        <tr>
                            <td style="width:202px">
                                <input type="text" maxlength="40" style="width: 100%; font-size: 11px;" name="q"
                                       id="q"/>
                            </td>
                            <td style="width:10px" rowspan="2">
                                <gsml:img src="/res/img/pixel.gif" alt="" style="width:6px;height:6px"/>
                            </td>
                            <td style="width:50px">
                                <gsweb:stateSelector allowNoState="true" noStateLabel="--" usingLongNames="false"
                                                     state="${requestScope.context.state}" styleClass="stateSelect"/>
                            </td>
                            <td rowspan="2" style="width:43px;vertical-align: top">
                                <gsml:url var="go" value="/res/img/header/btn_go.gif"/>
                                <input type="image" src="${go}" alt="go" style="width:37px;height:20px"
                                       class="button"/>
                            </td>
                        </tr>
                        <tr>

                            <td style="height:21px" class="searchRadios">
                                <input name="c" type="radio" value="school" checked="checked"
                                       onclick="setSearchPrompt('Enter school, district or city'); return true;"/><span>Schools</span>
                                <input name="c" type="radio" value="topic"
                                       onclick="setSearchPrompt('Enter keyword'); return true;"/>
                                <span>Articles</span>
                            </td>
                            <td style="height:21px">
                                &#160;
                            </td>
                        </tr>
                    </table>
                </gsml:form>
            </td>
            <td>&#160;</td>
        </tr>
    </table>
</td>
<td style="background-color: #fff; width: 10px;"><gsml:img src="/res/img/pixel.gif" alt=""
                                                           style="width:10px;height:50px"/></td>
</tr>
<tr>
    <td><gsml:img src="/res/img/pixel.gif" alt="" style="width: 18px"/></td>
    <td colspan="3">
        <table cellpadding="0" cellspacing="0" id="nav"><tr>
            <td style="width:16px">&#160;</td>
            <c:set var="pw" value="${requestScope.context.pathway}"/>
            <td><gsml:a href="/test/splash.page"
                        styleClass="${(pw == 0 ? 'lactive active' : 'inactive')} ${(pw == 1 ? 'ractive' : '')} "
                    >Home</gsml:a></td>
            <td><link:research
                    styleClass="${(pw == 0 ? 'lactive' : '')} ${(pw == 1 ? 'active' : 'inactive')} ${(pw == 3 ? 'ractive' : '')} "
                    >Research &amp;amp; Compare</link:research></td>
            <td><gsml:a href="/content/summerReading.page"
                        styleClass="${(pw == 1 ? 'lactive' : '')} ${(pw == 3 ? 'active' : 'inactive')} ${(pw == 4 ? 'ractive' : '')} "
                    >Summer Reading</gsml:a></td>
            <td><gsml:a href="/content/backToSchool.page"
                        styleClass="${(pw == 1 ? 'lactive' : '')} ${(pw == 7 ? 'active' : 'inactive')} ${(pw == 4 ? 'ractive' : '')} "
                    >Back to School</gsml:a></td>
            <td><gsml:a href="/about/aboutUs.page"
                        styleClass="${(pw == 3 ? 'lactive' : '')} ${(pw == 4 ? 'active' : 'inactive')} ${(pw == 5 ? 'ractive' : '')} "
                    >About Us</gsml:a></td>
            <td><link:contactUs
                    styleClass="${(pw == 4 ? 'lactive' : '')} ${(pw == 5 ? 'active' : 'inactive')} ${(pw == 6 ? 'ractive' : '')} "
                    >Contact Us</link:contactUs></td>
            <td><link:newsletterCenter
                    styleClass="${(pw == 5 ? 'lactive' : '')} ${(pw == 6 ? 'active ractive' : 'inactive')}"
                    email="${requestScope.context.user.email}"
                    >Free Newsletters</link:newsletterCenter></td>
            <td style="width:16px">&#160;</td>
        </tr></table>
    </td>
    <td><gsml:img src="/res/img/pixel.gif" alt="" style="width: 18px"/></td>
</tr>
<!--tr>
    <td class="frameLeft">&#160;</td>
    <td colspan="5"><div id="subnav">Subnav menu items go here</div></td>
    <td class="frameRight">&#160;</td>
</tr-->
</table>
</div>

</jsp:root>
