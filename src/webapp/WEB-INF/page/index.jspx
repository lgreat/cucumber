                         title=""/>
                    <img class="leadoff" src="/res/img/home/mother_daughter.jpg" alt="Mother and daughter"/>

                    <ul class="arrows">
                        <c:import
                                url="/content/splashLinks.module?page=latest_tools&amp;type=all&amp;layout=list&amp;&amp;omitUl=true"/>
                    </ul>

                    <div class="sponsor_ad"><ad:Homepage_185x28/></div>
                </div>
            </div>
        </div>
    </div>

    <div class="moduleBox">
        <div class="wrapper-outer">
            <div class="wrapper-inner">
                <div class="wrapper-content" style="width: 574px; height: 215px; position: relative;">
