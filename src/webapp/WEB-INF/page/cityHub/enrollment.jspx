<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:mod="urn:jsptagdir:/WEB-INF/tags/mod"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:hub="urn:jsptagdir:/WEB-INF/tags/cityHub"
          xmlns:gsweb="gstaglib"
          version="2.0">
    <jsp:directive.page contentType="text/html"/>
    <jsp:output doctype-root-element="html"
                doctype-public="-//W3C//DTD XHTML 1.0 Transitional//EN"
                doctype-system="http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"/>
    <html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
    <c:set var="pageName" value="Enrollment Page" scope="request"/>
    <head>
        <meta name="decorator" content="standardDecorator-hubs"/>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta name="description" content="${gsweb:enrollmentPageDescription(city)}"/>
        <meta name="keywords" content="${gsweb:enrollmentPageKeyWords(city)}"/>
        <title>${gsweb:enrollmentPageTitle(city)}</title>
        <gsml:javascript src="/res/js/cityHub/schoolSearch.js"/>
        <gsml:javascript src="/res/js/cityHub/genericTabs.js"/>
        <gsml:javascript src="/res/js/jquery.cycle2.min.js" />
        <gsml:javascript src="/res/js/jquery.cycle2.carousel.min.js" />
        <gsml:javascript src="/res/js/util/customCarouselControl.js"/>
    </head>

    <body>
    <hub:searchChoosing/>
    <hub:upcomingEvent hideAnnouncementModule="true"/>
    <hub:breadcrumbs city="${city}" state="${state}" pageName="Enrollment Information"/>
    <hub:enrollmentHeader/>
    <hub:enrollmentTabs/>
    <hub:enrollmentInfo/>
    <mod:tracking pageName="${pageName}" locale="${city}" hier1="${city} School Enrollment"
                  localPageName="${pageName}"/>
    </body>

    </html>
</jsp:root>