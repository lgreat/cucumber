<gsml:body adPageName="school/topHighSchoolsBadges">

    <!--<div id="topSchoolsWrapper">-->
        <div id="topSchoolsDownload">
            <link:topSchools state="${StateOrNull}" styleClass="underline">Top High Schools</link:topSchools> &gt; Winners' Badges
                <h2>Top Schools Winners: Download Award Badges Here!</h2>
                <p>Use the badges below to promote your achievement as a 2009 GreatSchools "Top School."</p>
                <p>Download the images and post them on your Web site.</p>
            
             <ul>
                  <li>
                      <img src="/res/img/school/topSchools/badge/badge_lowres_topperf.jpg"
                        alt="GreatSchools Top Schools, as featured in BusinessWeek"/>
                          <h3>Top Performing</h3>
                          <div><a href="/about/downloadLogo.page?badgeTypeAndName=topPerformingLowResJpg">Low-Res:JPEG</a></div>
                          <div><a href="/about/downloadLogo.page?badgeTypeAndName=topPerformingLowResPng">Low-Res:PNG</a></div>
                          <div><a href="/about/downloadLogo.page?badgeTypeAndName=topPerformingHighResJpg">High-Res:JPEG</a></div>
                          <div><a href="/about/downloadLogo.page?badgeTypeAndName=topPerformingHighResPng">High-Res:PNG</a></div>
                  </li>

                  <li>
                      <img src="/res/img/school/topSchools/badge/badge_lowres_improved.jpg"
                        alt="GreatSchools Top Schools, as featured in BusinessWeek"/>
                          <h3>Most Improved</h3>
                          <div><a href="/about/downloadLogo.page?badgeTypeAndName=mostImprovedLowResJpg">Low-Res:JPEG</a></div>
                          <div><a href="/about/downloadLogo.page?badgeTypeAndName=mostImprovedLowResPng">Low-Res:PNG</a></div>
                          <div><a href="/about/downloadLogo.page?badgeTypeAndName=mostImprovedHighResJpg">High-Res:JPEG</a></div>
                          <div><a href="/about/downloadLogo.page?badgeTypeAndName=mostImprovedHighResPng">High-Res:PNG</a></div>
                  </li>
                  <li>
                      <img src="/res/img/school/topSchools/badge/badge_lowres_pchoicepublic.jpg"
                        alt="GreatSchools Top Schools, as featured in BusinessWeek"/>
                          <h3>Parents' Choice, Public School</h3>
                          <div><a href="/about/downloadLogo.page?badgeTypeAndName=publicSchoolLowResJpg">Low-Res:JPEG</a></div>
                          <div><a href="/about/downloadLogo.page?badgeTypeAndName=publicSchoolLowResPng">Low-Res:PNG</a></div>
                          <div><a href="/about/downloadLogo.page?badgeTypeAndName=publicSchoolHighResJpg">High-Res:JPEG</a></div>
                          <div><a href="/about/downloadLogo.page?badgeTypeAndName=publicSchoolHighResPng">High-Res:PNG</a></div>
                  </li>
                  <li>
                        <img src="/res/img/school/topSchools/badge/badge_lowres_pchoiceprivate.jpg"
