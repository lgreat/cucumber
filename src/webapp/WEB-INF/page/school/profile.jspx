<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml" version="2.0"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:compare="urn:jsptagdir:/WEB-INF/tags/compare"
          xmlns:promo="urn:jsptagdir:/WEB-INF/tags/promo"
          xmlns:school="urn:jsptagdir:/WEB-INF/tags/school"
          xmlns:fn="http://java.sun.com/jsp/jstl/functions"
          xmlns:link="urn:www.greatschools.org/taglib/link"
          xmlns:gslink="urn:jsptagdir:/WEB-INF/tags/link"
          xmlns:profile="urn:jsptagdir:/WEB-INF/tags/school/profile"
          xmlns:gsweb="gstaglib"
          xmlns:ratings="urn:jsptagdir:/WEB-INF/tags/ratings"
          xmlns:mod="urn:jsptagdir:/WEB-INF/tags/mod"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:ad="urn:jsptagdir:/WEB-INF/tags/ad"
          xmlns:k12="urn:jsptagdir:/WEB-INF/tags/promo/k12"
          xmlns:communityHover="urn:jsptagdir:/WEB-INF/tags/community/hover/v1"
        >
<jsp:directive.page import="gs.data.school.School"/>
<jsp:directive.page import="gs.web.util.PageHelper"/>
<pageHelper:setPathway pathway="RESEARCH_COMPARE"/>



<jsp:output doctype-root-element="html"
            doctype-public="-//W3C//DTD XHTML 1.0 Transitional//EN"

            doctype-system="http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"/>
<jsp:directive.page contentType="text/html"/>
<html xmlns="http://www.w3.org/1999/xhtml" lang="en">

<c:set var="state" value="${school.databaseState}"/>
<c:set var="city" value="${school.physicalAddress.city}"/>
<c:set var="headerLinkAnchor" value="from..HeaderLink"/>


<!-- determine the page name that is reported to omniture -->
<c:set var="pageName" value="Overview" scope="request"/>
<c:choose>
    <c:when test="${school.preschoolOnly}">
        <c:set var="pageName" value="Preschool Overview" scope="request"/>
    </c:when>
    <c:when test="${school.type.schoolTypeName == 'private'}">
        <c:set var="pageName" value="Private School Overview" scope="request"/>
    </c:when>
</c:choose>
<jsp:scriptlet>
    PageHelper.setPageName(request, "spp_overview");
</jsp:scriptlet>

<head>
    <meta name="decorator" content="standardDecorator-v1"/>
    <meta name="description" content="${gsweb:schoolPageDescription(school)}"/>
    <meta name="keywords" content="${gsweb:schoolPageKeywords(school)}"/>
    <c:choose>
        <c:when test="${school.demoSchool}">
            <meta name="robots" content="noindex, nofollow, noarchive"/>
        </c:when>
    </c:choose>
    <c:set var="schoolPageTitle" value="${gsweb:schoolPageTitle(school)}"/>
    <title>${schoolPageTitle}</title>
    <c:if test="${not empty relCanonical}">
        <link rel="canonical" href="${relCanonical}"/>
    </c:if>


    <c:if test="${school.stateAbbreviation eq 'CA'}">
        <![CDATA[
            <!--[if lte IE 8]>
                <script type="text/javascript" src="/res/js/util/excanvas.js"></script>
            <![endif]-->
        ]]>
    </c:if>
    <gsml:combinedJavascript src="/res/js/school-profile.min.js">
        <!-- files to includes on dev workstations... must also exist in gsweb's pom.xml -->
        <gsml:javascript src="/res/js/json2.js"/>
        <gsml:javascript src="/res/js/history.min.js"/>
        <gsml:javascript src="/res/js/school/profilePage.js"/>
        <gsml:javascript src="/res/js/schoolReviewForm.js"/>
        <gsml:javascript src="/res/js/parentReviewTabSchoolReviewForm.js"/>
        <gsml:javascript src="/res/js/bootstrap/bootstrap-tooltip.js"/>
        <gsml:javascript src="/res/js/bootstrap/bootstrap-popover.js"/>
        <gsml:javascript src="/res/js/tracking/customLinks.js"/>
        <gsml:javascript src="/res/js/compare/persistentCompare.js"/>

        <!--Needed for photogallery-->
        <gsml:javascript src="/res/js/omnitureEventNotifier.js"/>
        <!--<gsml:javascript src="/res/js/photoGallery.js"/>-->

        <gsml:javascript src="/res/js/mssAutoHoverInterceptor.js"/>
    </gsml:combinedJavascript>



    <script type="text/javascript">
        var GS_PieLoader = [];
        $.getScript("https://www.google.com/jsapi", function() {
            google.load("visualization", "1", {packages:["corechart"], callback:function(){
                for (var x=0; x &lt; GS_PieLoader.length; x++) {
                    GS_PieLoader[x]();
                }
                GS_PieLoader = undefined;
            }});
        });
        $(document).ready(function(){
            GS.school.compare.initializeSchoolsInCompare(function() {
                return "sphead_overview_${school.databaseState}${school.id}";
            });
        });

    </script>

    <k12:trafficDriverIncludes/>

    <profile:facebookConfig showLongText="false" state="${school.databaseState}" schoolId="${school.id}"/>
    <ad:setSlotPrefix slotPrefix="School_Profile_Page"/>
    <ad:enableGptAsynchronousMode />
    <ad:setGptSlotLevelTargetingKeyAssignment adKeyword="template" jsLookupVariable="GS.ad.profile.tabNameForAdTargeting"/>
    <ad:disableGptGhostTextHiding position="Header_728x90"/>
    <ad:disableGptGhostTextHiding position="Footer_728x90"/>
    <ad:disableGptGhostTextHiding position="AboveFold_300x600"/>
    <ad:disableGptGhostTextHiding position="BelowFold_300x250"/>
    <ad:disableGptGhostTextHiding position="BelowFold_Top_300x125"/>
    <pageHelper:addFacebookMetaTags school="${school}" vpage="SCHOOL_PROFILE" />

    <!-- One of the modules is clearing the variable "overallRating" so I need to cache it here -->
    <!-- Urgent change for merge into 21.3. TODO: investigate why it is getting cleared -->
    <c:set var="overallRatingForMap" value="NR"/>
    <c:if test="${not empty overallRating}">
        <c:set var="overallRatingForMap" value="${overallRating}"/>
    </c:if>

</head>
<gsml:body adPageName="overview" pageLayout="overview">
<div class="container_24 clearfix">
<div class="phs clearfix">
<div id="fb-root"><!-- do not collapse --></div>
<!-- Breadcrumbs -->
<div class="grid_24 clearfix">
    <ul class="none smaller bottom mvs">
        <li class="di"><link:home>Home</link:home> &amp;gt; </li>
        <c:choose>
            <c:when test="${state eq 'DC'}">
                <![CDATA[<li class="di" itemscope itemtype="http://data-vocabulary.org/Breadcrumb">]]>
                <link:research state="${state}"
                               title="District of Columbia" markUpWithItemScopeUrl="true"><span
                        itemprop="title">District of Columbia</span></link:research> &amp;gt;
                <![CDATA[</li>]]>
                <![CDATA[<li class="di" itemscope itemtype="http://data-vocabulary.org/Breadcrumb">]]>
                <c:choose>
                    <c:when test="${school.levelCode eq 'p'}">
                        <link:schools cityName="${city}" state="${school.physicalAddress.state}" levelCode="p"
                                      title="Washington, D.C." markUpWithItemScopeUrl="true"><span
                                itemprop="title">Washington, D.C.</span></link:schools>&amp;#160;</c:when>
                    <c:otherwise>
                        <link:city city="${city}" state="${school.physicalAddress.state}"
                                   title="${city}" markUpWithItemScopeUrl="true"><span
                                itemprop="title">Washington, D.C.</span></link:city>&amp;#160;</c:otherwise>
                </c:choose>
                <![CDATA[</li>]]>
                <c:choose>
                    <c:when test="${not empty school.districtId and school.districtId eq 1}">
                        <li class="di">&amp;gt; <link:districtHome district="${school.district}"
                                                                   title="District information">DCPS</link:districtHome>
                        </li>
                    </c:when>
                    <c:when test="${school.districtId != 0 and school.levelCode != 'p'}">
                        <li class="di">&amp;gt; <link:districtHome district="${school.district}"
                                                                   title="District information"><c:out
                                value="${school.district.name}" escapeXml="true"/></link:districtHome>
                        </li>
                    </c:when>
                </c:choose>
            </c:when>
            <c:otherwise>
                <![CDATA[<li class="di" itemscope itemtype="http://data-vocabulary.org/Breadcrumb">]]>
                <c:choose>
                    <c:when test="${state eq 'NY' and city eq 'New York'}">
                        <link:research state="${state}"
                                       title="${state.longName}" markUpWithItemScopeUrl="true"><span
                                itemprop="title">${state.longName}</span></link:research> &amp;gt;
                    </c:when>
                    <c:otherwise>
                        <link:research state="${state}"
                                       title="${state.longName}" markUpWithItemScopeUrl="true"><span
                                itemprop="title">${state.longName}</span></link:research> &amp;gt;
                    </c:otherwise>
                </c:choose>
                <![CDATA[</li>]]>
                <![CDATA[<li class="di" itemscope itemtype="http://data-vocabulary.org/Breadcrumb">]]>
                <c:choose>
                    <c:when test="${school.levelCode eq 'p' and state eq 'NY' and city eq 'New York'}">
                        <link:schools cityName="${city}" state="${school.physicalAddress.state}" levelCode="p"
                                      title="${city}" markUpWithItemScopeUrl="true"><span
                                itemprop="title">NYC</span></link:schools>&amp;#160;</c:when>
                    <c:when test="${school.levelCode eq 'p'}">
                        <link:schools cityName="${city}" state="${school.physicalAddress.state}" levelCode="p"
                                      title="${city}" markUpWithItemScopeUrl="true"><span
                                itemprop="title">${city}</span></link:schools>&amp;#160;</c:when>
                    <c:when test="${state eq 'NY' and city eq 'New York'}">
                        <link:city city="${city}" state="${school.physicalAddress.state}"
                                   title="NYC" markUpWithItemScopeUrl="true"><span
                                itemprop="title">NYC</span></link:city>&amp;#160;</c:when>
                    <c:otherwise>
                        <link:city city="${city}" state="${school.physicalAddress.state}"
                                   title="${city}" markUpWithItemScopeUrl="true"><span
                                itemprop="title">${city}</span></link:city>&amp;#160;</c:otherwise>
                </c:choose>
                <![CDATA[</li>]]>
                <c:if test="${school.districtId != 0 and school.levelCode != 'p'}">
                    <li class="di">&amp;gt; <link:districtHome district="${school.district}"
                                                               title="District information"><c:out
                            value="${school.district.name}" escapeXml="true"/></link:districtHome>
                    </li>
                </c:if>
            </c:otherwise>
        </c:choose>
    </ul>
</div>
<!-- end: breadcrumbs -->

<!-- Begin profile header -->
<div class="hreview-aggregate grid_16 ptm">
    <!--This is used to stretch all the way across the header with the line, when no icon shows up.-->
    <c:set var="defaultClassHeaderWidth">alpha grid_16 omega</c:set>
    <c:set var="spriteString"><ratings:ratingsSprite isNewGsRating="${school.isNewGSRating}" gsRating="${overallRating}"
                                                     schoolType="${school.type.name}" isPreschoolOnly="${school.preschoolOnly}" spriteSize="large"/></c:set>
    <c:set var="largeOrangeDiamond" value="ratingx96-private-O r-96-NR" />
    <!--If the large orange diamond is going to show, don't do it-->
    <c:if test="${!fn:contains(spriteString, largeOrangeDiamond)}">
        <c:set var="defaultClassHeaderWidth">grid_12 omega</c:set>
        <gslink:linkToTab tabName="ratings" customLink="overview_ratings" cssClass="tdn make-555555 js-trigger">
            <div class="alpha grid_4 tac">
                <!-- New GS Rating -->
                ${spriteString}
                <p data-gs-custom-link="header_rating" class="tac small bottom pbs mbm">GreatSchools Rating</p>
            </div>
        </gslink:linkToTab>
    </c:if>
    <div class="${defaultClassHeaderWidth}">
        <!-- School name and sub data -->
        <h1 class="mbm"><gslink:linkToTab tabName="overview" cssClass="noUnderline"><span class="item"><span class="fn">${school.name}</span></span></gslink:linkToTab></h1>
        <div class="mbs" style="height:23px;">
            <span class="dib small bottom prm" style="line-height: 24px;">
            <!-- School Type -->
            <c:if test="${school.levelCode ne 'p'}">
                ${gsweb:capitalize(school.type.schoolTypeName)}
            </c:if>
            <!-- Grade levels -->
            <c:choose>
                <c:when test="${school.levelCode eq 'p'}">
                    Preschool
                </c:when>
                <c:when test="${empty school.gradeLevels or school.gradeLevels eq 'n/a' or school.gradeLevels eq 'N/A'}">
                    <!-- Leave blank -->
                </c:when>
                <c:otherwise>
                    <span class="make-dddddd phs">|</span> ${school.gradeLevels}
                </c:otherwise>
            </c:choose>
            <!-- Enrollment -->
            <c:if test="${not empty schoolEnrollment and schoolEnrollment > 0}">
                <span class="make-dddddd phs">|</span> ${schoolEnrollment} ${gsweb:pluralize(schoolEnrollment, 'student')}
            </c:if>
            </span>

            <!-- Pinterest Button : uses parameters set on addFacebookMetaTags.tagx -->
            <div id="schoolProfilePinIt" style='vertical-align: top; display: inline-block; *display: inline;'>

                <promo:addPinItButton url="${pinterestUrl}" title="${schoolPlainTextSummary}"
                                      image="http://www.greatschools.org/res/img/content/article/GS_FB_logo_200x200.jpg"/>
            </div>

            <!-- Facebook 'Like' Button -->
            <c:if test="${not empty facebook['facebookUrl']}">

            <![CDATA[
                <fb:like href="${facebook['facebookUrl']}" layout="button_count" show_faces="false" width="90" action="like" colorscheme="light" font="arial"></fb:like>
            ]]>
            </c:if>
            <!--<promo:facebookLikeButton showLongText="false" state="${school.databaseState}" schoolId="${school.id}"/>-->
        </div>

        <hr class="keyline1"/>

        <!-- tool links -->
        <div class="toolLinks">
            <ul class="none small mtm">
                <li>
                <span class="media">
                    <span class="img iconx16 i-16-save mrs"><!-- do not collapse --></span>
                    <c:choose>
                        <c:when test="${not empty schoolInMSL and schoolInMSL eq true}">
                            Added to <link:mySchoolList>My School List</link:mySchoolList>
                        </c:when>
                        <c:otherwise>
                            <link:mySchoolList styleClass="bd" school="${school}"
                                               onclick="return GS.showAddMslJoinHover(null, '${gsweb:escapeJavaScript(school.name)}', ${school.id}, '${school.databaseState}', this);"
                                               anchor="${headerLinkAnchor}">Add to My School List</link:mySchoolList>
                        </c:otherwise>
                    </c:choose>
                </span>
                </li>
                <c:if test="${school.type.schoolTypeName ne 'private'}">
                    <li class="mll">
                    <span class="media">
                        <span class="img iconx16 i-16-email mrs"><!-- do not collapse --></span>
                        <link:newsletterManagement schoolId="${school.id}" schoolState="${school.databaseState}" styleClass="bd"
                                                   onclick="return GS.showMssJoinHover(this.href, '${gsweb:escapeJavaScript(school.name)}', ${school.id}, '${school.databaseState}');">Send me updates</link:newsletterManagement>
                    </span>
                    </li>
                </c:if>
                <li class="mll">
                <span class="media">
                    <span class="img iconx16 i-16-review mrs"><!-- do not collapse --></span>
                    <gslink:linkToTab tabName="reviews" anchor="schoolReviewSubmitForm" cssClass="bd">Rate &amp; Review</gslink:linkToTab>
                </span>
                </li>
            </ul>
        </div>

    </div>
    <div class="clear"><!-- do not collapse --></div>

    <!-- - - - - - - - - - -->
    <!-- TAB CONFIGURATION -->
    <!-- - - - - - - - - - -->

    <jsp:useBean id="schoolProfileTabMap" class="java.util.HashMap" scope="request" />
    <c:set target="${requestScope.schoolProfileTabMap}" property="key" value="on"/>

    <c:set target="${requestScope.schoolProfileTabMap}" property="${param.tab}" value="selected"/>

    <!-- Configure which sub-tabs live under which master tabs -->
    <!-- IMPORTANT: if you make any changes to the tabs below, make sure to update profilePage.js - GS.ad.profile.tabNameForAdTargeting -->
    <c:if test="${empty param.tab or param.tab eq 'overview'}">
        <c:set target="${requestScope.schoolProfileTabMap}" property="overview-master" value="selected"/>
    </c:if>
    <c:if test="${param.tab eq 'reviews'}">
        <c:set target="${requestScope.schoolProfileTabMap}" property="reviews-master" value="selected"/>
    </c:if>
    <c:if test="${param.tab eq 'test-scores' or param.tab eq 'ratings'}">
        <c:set target="${requestScope.schoolProfileTabMap}" property="test-scores-master" value="selected"/>
    </c:if>
    <c:if test="${param.tab eq 'demographics' or param.tab eq 'teachers'}">
        <c:set target="${requestScope.schoolProfileTabMap}" property="students-teachers-master" value="selected"/>
    </c:if>
    <c:if test="${param.tab eq 'programs-culture' or param.tab eq 'culture' or param.tab eq 'programs-resources' or param.tab eq 'extracurriculars'}">
        <c:set target="${requestScope.schoolProfileTabMap}" property="programs-master" value="selected"/>
    </c:if>
    <c:if test="${param.tab eq 'enrollment'}">
        <c:set target="${requestScope.schoolProfileTabMap}" property="enrollment-master" value="selected"/>
    </c:if>

    <!-- - - - - - - - - - - - -->
    <!-- END TAB CONFIGURATION -->
    <!-- - - - - - - - - - - - -->

    <profile:primary schoolName="${school.name}" schoolId="${school.id}" schoolState="${school.databaseState}" tabName="${param.tab}"/>
</div>

<div class="grid_8">

    <!-- Map -->

    <c:set var="schoolAddress" value="${gsweb:escapeHtml(school.physicalAddress.asSingleLineString)}"/>
    <c:set var="schoolAddressWithName"
           value="${schoolAddress}+(${gsweb:escapeHtml(school.name)})"/>

    <!-- ratings:ratingsMapPinUrl sets the mapPinUrl variable which is used below. -->
    <ratings:ratingsMapPinUrl school="${school}"/>
    <c:set var="mapMarkerParam" value="icon:http://www.greatschools.org${mapPinUrl}|${schoolAddress}"/>

    <div class="media">
        <div class="img mrm">
            <div class="mod standard_1-1-1 mbs">
                <div class="inner">
                    <div class="box-shadow-animation-1">
                        <link:districtBoundaryTool school="${school}" onclick="if (s.tl) { s.tl(this, 'o', 'header_largermap'); }">
                            <div class="pr">
                                <img height="98" width="98" alt="Map thumbnail"
                                 src="http://maps.googleapis.com/maps/api/staticmap?markers=${gsweb:urlEncode(mapMarkerParam)}&amp;size=98x98&amp;maptype=roadmap&amp;scale=1&amp;sensor=false"/>
                                <div style="position:absolute; top:73px; right:1px;z-index: 0;"><span class="iconx24 i-24-map-plus"><!-- do not collapse --></span></div>
                            </div>
                        </link:districtBoundaryTool>
                    </div>
                </div>
            </div>

            <p class="smaller">
                <a class="bd smaller bottom" onclick="if (s.tl) { s.tl(this, 'o', 'School Profile - Get Directions'); }" href="http://maps.google.com/?daddr=${schoolAddressWithName}"
                   target="_blank">Get directions &amp;raquo;</a>
                <!--<link:districtBoundaryTool school="${school}" onclick="if (s.tl) { s.tl(this, 'o', 'header_largermap'); }">Larger map</link:districtBoundaryTool>-->
            </p>
        </div>


        <!-- Address, phone, fax, directions, homes for sale -->
        <div class="bd fl" style="width:200px;">
            <p class="small bottom ">${school.physicalAddress.street}

                <c:if test="${not empty school.physicalAddress.streetLine2 and school.physicalAddress.streetLine2 ne ','}">
                    <br/>
                    ${school.physicalAddress.streetLine2}
                </c:if>

                <br/>
                <span>${school.physicalAddress.city}</span>,
                <span>${school.physicalAddress.state.abbreviation} </span>
                <span>${school.physicalAddress.zip}</span>



                <!--<c:if test="${not empty school.fax}">-->
                    <!--<br/>-->
                    <!--<strong>Fax: </strong>${school.fax}-->
                <!--</c:if>-->

                <c:if test="${not empty school.webSitePrettyUrl or not empty k12AffiliateUrl}">
                    <c:set var="schoolWebsiteUrl" value="${not empty k12AffiliateUrl ? k12AffiliateUrl : school.webSitePrettyUrl}"/>
                    <br/>
                    <strong>Website: </strong><a href="${schoolWebsiteUrl}" target="_blank">Click here</a>
                </c:if>
                <c:if test="${not empty school.phone}">
                    <br/>
                    <strong>Phone: </strong>${school.phone}
                </c:if>
                <c:if test="${requestScope.context.showRealtorDotComPromos}">
                    <gslink:realtorDotComLink address="${school.physicalAddress}"
                                              customLinkSuffix="inlinelinkheader" omniturePageName="${pageName}">Nearby homes for sale</gslink:realtorDotComLink>
                </c:if>
            </p>

            <!--<div>-->
                <!--<div class="media">-->
                    <!--<span class="img iconx16 i-16-getDirections mrs">&lt;!&ndash; do not collapse &ndash;&gt;</span>-->
                    <!--<a class="bd small bottom" onclick="if (s.tl) { s.tl(this, 'o', 'School Profile - Get Directions'); }" href="http://maps.google.com/?daddr=${schoolAddressWithName}"-->
                       <!--target="_blank">Get directions</a>-->
                <!--</div>-->


            <!--</div>-->
        </div>
    </div>

    <div class="">
        <ad:AboveFold_300x600 />

        <promo:emailSignUp-v1 userIsAlreadySignedUp="false" />

        <compare:persistentCompare school="${school}"/>
        <school:profileNearby nearbySchools="${nearbySchools}" />

        <promo:searchRealtorDotCom omniturePageName="${pageName}" pageAdRatioKey="RDC_PCT_ARTICLE" showAdPct="0" style="ND" size="300x137" defaultCity="${school.physicalAddress.city}" defaultState="${school.physicalAddress.state}"/>

        <ad:adTag position="Community_Ad_300x50"/>

        <c:if test="${not empty school and not empty school.physicalAddress and not empty school.physicalAddress.city and
        ((fn:toLowerCase(school.physicalAddress.city) eq 'indianapolis') or (fn:toLowerCase(school.physicalAddress.city) eq 'washington')
        or (fn:toLowerCase(school.physicalAddress.city) eq 'milwaukee'))}">
            <school:profileChooser city="${school.physicalAddress.city}" state="${school.physicalAddress.state}"/>
        </c:if>

        <ad:BelowFold_300x250/>

        <c:set var="isDcNclbSchool" value="${school.physicalAddress.state eq 'DC' and (school.type.schoolTypeName eq 'public' or school.type.schoolTypeName eq 'charter') and school.levelCode ne 'p'}"/>
        <c:if test="${isDcNclbSchool and requestScope.context.showDcNclbModules}">
            <!-- WARNING: dcNclbReportCard.tagx IS SHARED WITH OTHER PAGES, Bracket all changes under style eq '2012' -->
            <school:dcNclbReportCard
                    school="${school}"
                    showAllSchoolLinks="${requestScope.context.showDcNclbAllSchoolLinks}"
                    schoolCodesToShow="${requestScope.context.dcNclbSchoolCodesToShow}"
                    districtCodesToShow="${requestScope.context.dcNclbDistrictCodesToShow}"
                    style="2012"
                    />
        </c:if>

        <ad:BelowFold_Top_300x125/>

    </div>
</div>

<div style="display: none;">
    <mod:tracking pageName="If you see this being sent to omniture, something is wrong"
                  hier1="If you see this being sent to omniture, something is wrong"
                  school="${school}" augmentInitialRequestWithDataForTab="${not empty param.tab? param.tab : 'overview'}"/>
</div>

<communityHover:clickToReviewYourSchool schoolName="${not empty school ? school.name : 'your school'}"/>
<school:hovers2012 school="${school}" tab="${not empty param.tab? param.tab : 'overview'}"/>
</div>
</div>
</gsml:body>
</html>
</jsp:root>