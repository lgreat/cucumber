    <!-- Any adjustment to left column heights in CSS will require a change here! -->
    <!-- 25 is bottom padding, 278 is height of editor's picks section plus 10px margin, 10 is top padding -->
    <script type="text/javascript">
        function adjustLeftColAd() {
            var rightcol = document.getElementById('right');
            var rightHeight = rightcol.scrollHeight;
            var leftcolAd = document.getElementById('leftcolAd');
            leftcolAd.style.height = (rightHeight-25-278-10)+'px';
        }

        // Returns true if a state has been selected.
        function checkStateSelection(id) {
            var select = document.getElementById(id);
            if (select.options[select.selectedIndex].value == "") {
                alert("Please select a state");
                return false;
            } else {
                return true;
            }
        }
    </script>
