            /* Headings
           -------------------------------------------------------------- */

        h1,
        h2,
        h3,
        h4,
        h5,
        h6 { font-weight: normal; color: #2b2b2b; }
            /*h1 {color: #FF8000; }*/

        h1 { font: normal 24px/30px Arial, Helvetica, sans-serif; margin-bottom: 20px; }
        h2 { font: normal 21px/25px Arial, Helvetica, sans-serif; margin-bottom: 2px; }
        h3 { font: normal 14px/20px Arial, Helvetica, sans-serif; margin-bottom: 2px; color: blue;}

        h4 { font: normal 16px/20px Arial, Helvetica, sans-serif; margin-bottom: 20px; }
        h5 { font: bold   14px/20px Arial, Helvetica, sans-serif; margin-bottom: 20px; }
        h6 { font: normal 14px/20px Arial, Helvetica, sans-serif; margin-bottom: 20px; }

        h1 img,
        h2 img,
        h3 img,
        h4 img,
        h5 img,
        h6 img { margin: 0; }

        hr.keyline1 {background:#dddddd;color:#dddddd;clear:both;float:none;width:100%;height:1px;margin:0;border:none;}
        hr.keyline2 {background:#e6e6e6;color:#e6e6e6;clear:both;float:none;width:100%;height:1px;margin:0;border:none;}
        hr.space {background:#fff;color:#fff;visibility:hidden;}


