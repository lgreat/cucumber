<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="1.2"
          xmlns="http://www.w3c.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:fmt="http://java.sun.com/jsp/jstl/fmt"
          xmlns:spring="http://www.springframework.org/tags"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:mod="urn:jsptagdir:/WEB-INF/tags/mod"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:ad="urn:jsptagdir:/WEB-INF/tags/ad"
          xmlns:gsweb="gstaglib"
        >

<jsp:directive.page import="gs.web.util.PageHelper"/>
<pageHelper:setPathway pathway="RESEARCH_COMPARE"/>
<html lang="en">

<jsp:output doctype-root-element="html"
            doctype-public="-//W3C//DTD XHTML 1.0 Transitional//EN"
            doctype-system="http://www.w3c.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"/>

<jsp:directive.page contentType="text/html"/>
<head>
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8"/>
    <jsp:scriptlet>
        PageHelper.hideHeader(request);
        PageHelper.hideFooter(request);
        PageHelper.hideLeaderboard(request);
    </jsp:scriptlet>
    <title>Select a state</title>
    <style type="text/css">
        #marginGS {padding:0; margin:0;width: 450px;font-size:12px;}
        h2 {font-weight: bold; font-size: 18px; text-align: left; color: #66cccc; padding:0; margin: 0 0 5px 0; font-family:"Trebuchet MS", sans-serif;line-height:100%;}
        select {color:#696969;width:190px;font-size:11px;height:200px;font-weight:bold;padding-bottom:0;margin-bottom:0;}
        select option {padding:0}
    </style>
    <base target="_parent"/>
</head>

<c:set var="url" value="/stateLauncher.page"/>

<body>
    <div style="height:360px;padding-left:20px;padding-top:10px;background-color:#fff;">
        <h2>Please<br/>select your state:</h2>


        <gsweb:form action="${url}" target="_parent" method="get">
            <select size="12" name="state" id="state">
