            <div style="padding-left:69px;padding-top:0;margin-top:0;">
                <a onclick="window.top.hidePopWin(false);"><gsml:img src="res/img/button/btn_cancel_aaccdd.gif" alt="Cancel"/></a>&amp;#160;&amp;#160;
                <gsml:inputImage src="res/img/button/btn_go_aaccdd.gif" alt="Go"/>
            </div>
        </gsweb:form>
    </div>

<mod:tracking pageType="search" pageName="State Selection Hover" hier1="Search,State select page,Hover"/>
</body>
</html>
</jsp:root>
