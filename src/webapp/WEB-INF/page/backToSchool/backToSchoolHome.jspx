                    }
                 -->
                </script>
                <noscript>

                        <img src="/res/img/countdown/sample.gif" alt="Countdown sample"/>

                </noscript>
                ]]>
            </c:when>
            <c:otherwise>
                <gsweb:adTagGenerator position="countdown_b2s_311x250"/>
            </c:otherwise>
                 </c:choose>

            </div>


            <div id="momOnFire">
            <div class="momWrapper">
                <div class="blogHeader">Blog on Fire</div>
                <content:blogFeed atomUrl="http://blogs.greatschools.org/mom_on_fire/atom.xml"
                                              defaultTitle="Mom on Fire"
                                              defaultUrl="http://blogs.greatschools.org/mom_on_fire"
                                              type="momOnFire"/>
                 <div id="momSponsor"><gsweb:adTagGenerator position="sponsor_90x45" /></div>
                </div>
            </div>
            <div id="hotProducts"><jsp:include page="/promo/hotProducts.module?slotPrefix=Library_Microsite_Back_to_School"/></div>
             <div id="quiz">
                 <c:choose>
            <c:when test="${requestScope.pageHelper.adFree or requestScope.pageHelper.framed or requestScope.pageHelper.adServedByCobrand}">
               <a href="/school/parentReview.page" target="_parent"><img src="/res/img/backToSchool/parentReviewHouseAd311x250.jpg" alt="Parent Review Img"></img></a>
            </c:when>
            <c:otherwise>
                <gsweb:adTagGenerator position="BelowFold_311x250"/>
            </c:otherwise>
                 </c:choose>
                 </div>

            <div id="community">
                <div class="communityWrapper">
                    <h3>Community</h3>
                    <c:if test="${communityTopicsSize gt 0}">
                    <ul class="communityUl">
                        <c:forEach var="topic" begin="0" end="${communityTopicsSize-1}">
                            <li class="linksTextList"><a
                                    href="${communityTopics[topic].value}">${communityTopics[topic].key}</a>
                                <div class="linksDivider">
                                    <hr/>
                                </div>
                            </li>
                        </c:forEach>
                    </ul>
                    </c:if>
                    <div class="additionalLinks"><a href="http://${communityHost}">More discussions &gt;</a></div>
                </div>
            </div>
        </div>
        <div id="contentRight">
             <c:choose>
            <c:when test="${requestScope.pageHelper.adFree or requestScope.pageHelper.framed or requestScope.pageHelper.adServedByCobrand}">
               <a href="http://www.facebook.com/greatschools" target="_blank"><img src="/res/img/backToSchool/fb_gs_house_300_137_img.jpg" alt="facebook Img"></img></a>
            </c:when>
            <c:otherwise>
                <gsweb:adTagGenerator position="House_Ad_300x137"/>
            </c:otherwise>
                 </c:choose>

            <ad:AboveFold_300x250/>
            <jsp:include page="/backToSchool/popularArticles.module"/>
            <gsweb:adTagGenerator position="House_Ad_300x137_B2S" alwaysShow="true"/>
            <!--
            <jsp:include page="/content/donorsChoose.module">
                <jsp:param name="maxProposals" value="${3}"/>
                <jsp:param name="omnitureCustomLink" value="DonorsChoose_backToSchool"/>
            </jsp:include>
            -->
        </div>
        
        <div style="display: none;">
            <mod:tracking pageName="${pageName}"
            hier1="Article,Back to School"
            microsite="${pageName}"/>
        </div>

    </gsml:body>
    </html>
</jsp:root>