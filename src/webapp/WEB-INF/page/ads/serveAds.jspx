<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="1.2"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:ad="urn:jsptagdir:/WEB-INF/tags/ad"
        >
    <jsp:directive.page contentType="text/html"/>

    <jsp:scriptlet>
        String[] ads = request.getParameterValues("ad");
        String prefix = request.getParameter("slotPrefix");
        if (ads != null) {
            for (int x=0; ads.length > x; x++) {
                request.setAttribute("curAd", ads[x]);
                request.setAttribute("slotPrefix", prefix);
                </jsp:scriptlet>
                <ad:setSlotPrefix slotPrefix="${slotPrefix}"/>

                <c:choose>
                    <c:when test="${curAd eq 'Header_728x90'}">
                        <Header_728x90>
                            <ad:Header_728x90/>
                        </Header_728x90>
                    </c:when>
                    <c:when test="${curAd eq 'Top_300x137'}">
                        <Top_300x137>
                            <ad:Top_300x137/>
                        </Top_300x137>
                    </c:when>
                    <c:when test="${curAd eq 'House_Ad_300x137'}">
                        <House_Ad_300x137>
                            <ad:House_Ad_300x137 referredBy=""/>
                        </House_Ad_300x137>
                    </c:when>
                    <c:when test="${curAd eq 'BelowFold_Top_300x125'}">
                        <BelowFold_Top_300x125>
                            <ad:BelowFold_Top_300x125 blurb="Advertiser Links"/>
                        </BelowFold_Top_300x125>
                    </c:when>
                    <c:when test="${curAd eq 'AboveFold_300x250'}">
                        <AboveFold_300x250>
                            <ad:AboveFold_300x250/>
                        </AboveFold_300x250>
                    </c:when>
                    <c:when test="${curAd eq 'Homepage_300x250'}">
                        <Homepage_300x250>
                            <ad:Homepage_300x250/>
                        </Homepage_300x250>
                    </c:when>
                    <c:when test="${curAd eq 'BelowFold_300x250'}">
                        <BelowFold_300x250>
                            <ad:BelowFold_300x250/>
                        </BelowFold_300x250>
                    </c:when>
                    <c:when test="${curAd eq 'Footer_728x90'}">
                        <Footer_728x90>
                            <ad:Footer_728x90/>
                        </Footer_728x90>
                    </c:when>
                    <c:when test="${curAd eq 'AboveFold_Left_160x600'}">
                        <AboveFold_Left_160x600>
                            <ad:AboveFold_Left_160x600/>
                        </AboveFold_Left_160x600>
                    </c:when>
                    <c:when test="${curAd eq 'Inline_460x94'}">
                        <Inline_460x94>
                            <ad:Inline_460x94/>
                        </Inline_460x94>
                    </c:when>
                    <c:when test="${curAd eq 'Promo_170x92'}">
                        <Promo_170x92>
                            <ad:Promo_170x92/>
                        </Promo_170x92>
                    </c:when>
                    <c:when test="${curAd eq 'Global_Nav_Promo_865x24'}">
                        <Global_Nav_Promo_865x24>
                            <ad:Global_Nav_Promo_865x24/>
                        </Global_Nav_Promo_865x24>
                    </c:when>
                    <c:when test="${curAd eq 'Global_Nav_Promo_952x24'}">
                        <Global_Nav_Promo_952x24>
                            <ad:Global_Nav_Promo_952x24/>
                        </Global_Nav_Promo_952x24>
                    </c:when>
                </c:choose>
                <jsp:scriptlet>
            }
        }
    </jsp:scriptlet>
</jsp:root>