                    <form class="forms treb12 tint777">
                        <div>
                            <img src="/res/img/content/research/yellow_arrow.gif" alt="yellow arrow"/>&amp;nbsp;
                            <label for="level">Compare D.C.</label>&amp;nbsp;
                            <select id="compareLevel" class="inputs tint666" name="level">
                                <option value="e">elementary schools</option>
                                <option value="m">middle schools</option>
                                <option value="h">high schools</option>
                            </select></div>
                        <div class="spacer_4"><!-- do not collapse --></div>
                        <div>
                            <label for="miles">within</label>&amp;nbsp;
