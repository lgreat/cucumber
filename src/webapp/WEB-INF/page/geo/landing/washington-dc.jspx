<head>
    <meta content="text/html;charset=utf-8" http-equiv="Content-Type"/>
    <title>Choosing a School in Washington, DC | GreatSchools </title>
    <meta name="description"
          content="Local program that helps parents research and choose the best schools in Washington, DC"/>
    <meta name="keywords"
          content="Washington DC school choice, Washington DC school guide, DC School Chooser, choosing a school Washington DC, GreatSchools DC, GSDC"/>
    <pageHelper:externalCss file="/res/css/geo/landing.css"/>
    <pageHelper:externalCss file="/res/css/geo/landing-ie7.css" userAgentSubstring="IE "/>
    <pageHelper:externalCss file="/res/css/geo/washington-dc.css"/>
    <jsp:scriptlet>
        SessionContext sessionContext = SessionContextUtil.getSessionContext(request);
        sessionContext.setState(State.DC);
    </jsp:scriptlet>
    <script type="text/javascript">
        function gotoDCCityPage() {
            window.location.href = '/washington-dc/washington/';
        }
        function gotoDCSchoolsPage() {
            var url = "/washington-dc/washington/";
            url += document.getElementById('findAllLevel').value;
            window.location.href = url;
        }
        function gotoComparePage() {
            var url = "/cgi-bin/cs_compare/dc/?sortby=distance%26tab=over%26area=m%26zip=";
            url += document.getElementById('zip').value;
            url += "%26miles=";
            url += document.getElementById('miles').value;
            url += "%26level=";
            url += document.getElementById('compareLevel').value;
            window.location.href = decodeURIComponent(url);
        }
    </script>
</head>

