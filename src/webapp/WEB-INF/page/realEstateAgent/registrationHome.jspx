<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:form="http://www.springframework.org/tags/form"
          xmlns:mod="urn:jsptagdir:/WEB-INF/tags/mod"
          xmlns:realEstate="urn:jsptagdir:/WEB-INF/tags/realEstateAgent"
          version="2.0">
    <jsp:directive.page import="gs.web.util.CookieUtil"/>

    <html>
    <jsp:directive.page contentType="text/html"/>
    <jsp:scriptlet>
        request.setAttribute("isHubUser", CookieUtil.getCookieValue(request, "ishubUser"));
    </jsp:scriptlet>

    <head>
        <c:choose>
            <c:when test="${not empty isHubUser}">
                <meta name="decorator" content="standardDecorator-hubs"/>
            </c:when>
            <c:otherwise>
                <meta name="decorator" content="standardDecorator-v1"/>
            </c:otherwise>
        </c:choose>
        <meta name="description" content="GreatSchools Radar makes it easy for real estate pros to create customized 
        neighborhood school guides for their home listings"/>
        <meta name="keywords" content="school reports, school guides, real estate schools, school reports for real
        estate, GreatSchools Radar"/>

        <title>Neighborhood School Guides, Real Estate Solutions | GreatSchools</title>
        <gsml:javascript src="/res/js/plupload-new/moxie.min.js" />
        <gsml:javascript src="/res/js/plupload-new/plupload.js" />
        <!--<gsml:javascript src="/res/js/plupload-new/plupload.flash.js" />-->

        <gsml:javascript src="/res/js/mediaUploader/MediaUploader.js" />
        <gsml:javascript src="/res/js/realEstateAgent/RealEstateAgentMediaUploader.js" />
        <gsml:javascript src="/res/js/realEstateAgent/registration.js"/>
        <jsp:scriptlet>
            java.util.List stateList = gs.data.state.StateManager.getList();
            request.setAttribute("stateList", stateList);
        </jsp:scriptlet>
    </head>
    <body>
    <div class="imageBgScale" style="height: 400px;">
        <div class="img-container">
            <img class="scaling" src="/res/img/realEstate/hero_realtors_1580x650.png"
                 alt="Real estate background image"/>
        </div>
        <div class="content-centered moduleTop-125">
            <p style="font-family: 'SourceSansPro-Semibold' arial; font-size:56px; color: #FFFFFF; padding: 50px 0 5px 0; text-shadow: 0 0 24px rgba(0,0,0,1)">GreatSchools Radar <sup style="font-family: 'SourceSansPro-It' arial;font-style: italic; font-weight: normal; font-size: 20px;">beta</sup></p>
            <p style="font-family: 'SourceSansPro-It' arial; font-size:20px; font-style: italic; color: #FFCE7A; padding-bottom: 60px; text-shadow: 0 0 18px rgba(0,0,0, 01);">Customizable neighborhood school guides for real estate pros</p>
        </div>
    </div>
    <div class="container_24 clearfix pr">
        <div class="pa" style="width:283px;top: -37px; left:352px;">
            <div class="tac outerBox">
                <a href="javascript:void(0)">
                    <button class="but but-xl but-1" name="registerBtn" id="jq-realEstAgentRegisterBtn">
                        Sign up now
                    </button>
                </a>
            </div>
        </div>
        <div class="pa" style="right: 108px; top: 20px;">
            <img src="/res/img/realEstate/arrow_callout.png" alt="Free during beta!"/>
        </div>
    </div>
    <div class="clearfix" style="padding: 80px 0 34px 0;">
        <div class="container_24">
            <div class="grid_8">
                <img src="/res/img/realEstate/family_300x180.png" alt="Family"/>
                <h3 class="mtl ptm">Dazzle your clients with neighborhood school guides</h3>
                <p class="medium mrm">Your clients want to know about the quality of schools near a home they are considering, but as a real estate agent, the law limits what you can share about neighborhoods. Impress your clients with our school guides and close the deal.</p>
            </div>
            <div class="grid_8">
                <img src="/res/img/realEstate/imac_300x180.png" alt="Imac"/>
                <h3 class="mtl ptm">Trusted school information from the<br/>leading independent source</h3>
                <p class="medium mrm">GreatSchools is a national non-profit that offers the most comprehensive set of quality school data available. Leading real estate websites such as Zillow, Trulia and Realtor.com trust us to power their school data.</p>
            </div>
            <div class="grid_8">
                <div class="pr">
                <a href="/res/pdf/realEstate/GreatSchoolsRadar_example.pdf" target="_blank" class="pa" style="width:30px; height:30px; top:0; right:10px;background-image:url(/res/img/pixel.gif);"><!--Do not collapse--></a>
                <img src="/res/img/realEstate/pdf_300x180.png" alt="PDF"/>
                </div>
                <h3 class="mtl ptm">Customized for your listings, with<br/>unlimited access</h3>
                <p class="medium mrm">Create professional full-color neighborhood school guides for all of your listings – as many as you need. In just a few easy steps you’ll get a printable PDF instantly, tailored to your clients and branded with your contact information.</p>
            </div>
        </div>
    </div>
    <div class="clearfix" style="border-top: 1px solid #DDDDDD; background-color: #F1F1F1; padding: 60px 0;">
        <div class="container_24">
            <h2>About GreatSchools</h2>
            <p class="bottom">Founded in 1998, GreatSchools is the leading provider of school information for communities across the U.S. Comprehensive school data is updated regularly from federal, state and local sources. We boast over 200,000 PreK-12 school profiles and more than one million insightful school reviews and ratings, provided by parents, teachers and students. In the past year half of U.S. families with school-aged children visited GreatSchools.org. </p>
        </div>
    </div>
    <div id="js-registrationHover" class="dn">
        <div class="mod modal_4 width_12 js-registrationHover" id="js-registrationHover">
            <div class="modalClose">
                <span class="iconx24 i-24-yellow-modal-close js_closeHover"><!-- do not collapse --></span>
            </div>
            <div class="outer">
                <div class="inner">
                    <realEstate:personalInfoHover/>
                    <realEstate:businessInfoHover/>
                    <realEstate:imageUploadHover/>
                </div>
            </div>
        </div>
    </div>
    <div style="display: none;">
        <mod:tracking pageName="Radar Landing" hier1="real-estate, GreatSchools Radar, Landing"/>
    </div>
    </body>
    </html>
</jsp:root>
