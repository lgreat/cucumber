<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:realEstate="urn:jsptagdir:/WEB-INF/tags/realEstateAgent"
        >
    <html>
    <jsp:directive.page contentType="text/html"/>

    <c:set var="countPerPage" value="5"/>

    <head>
        <title>Real Estate School Report | GreatSchools</title>
        <meta name="decorator" content="standardDecorator-v1"/>
        <link href="/res/css/15_pdf/realEstate.css" rel="stylesheet" type="text/css" />
        <!--<link href="/res/css/06_sprites/icon.css" rel="stylesheet" type="text/css" />-->
    </head>
    <body>

    <div class="divFooter realEstatePDF clearfix">
        <div class="fl">
            <span class="smallBold make-555555">Prepared for: </span>
            <span class="small make-555555"> ${agentFirstName} ${agentLastName}, ${agentEmail} </span>
        </div>
        <div class="fr">
            <span class="small make-555555">${date}</span>
        </div>
    </div>
    <div class="divHeader">
    <div class="realEstatePDF clearfix">
        <div class="" style="">
            <img src="/res/img/logo/logo_GS_200x50.png" class="fr" alt="greatschools"/>
        </div>
        <div class="mod-2">
            <div class="inner">
                <div class="hd mediumBold">
                    Your Neighborhood School Guide for:
                </div>
                <div class="bd">
                    <div class="xlargeBold">${streetNumber} ${streetName}</div>
                    <div class="defaultBold make-555555">
                        ${city}, ${state} ${zipcode}
                        <c:if test="${not empty bed}">
                            <span class="header-vertical-border-l">
                                <span class="make-999999"> | </span>
                                Bedrooms: ${bed}<span class="make-999999"> | </span></span>
                        </c:if>
                        <c:if test="${not empty bath}">
                        <span class="header-vertical-border-l">Bathrooms: ${bath}<span
                                class="make-999999"> | </span></span>
                        </c:if>
                        <c:if test="${not empty sqFeet}">
                            <span class="header-vertical-border-l"> ${sqFeet} square feet</span>
                        </c:if>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
        <!-- end of header -->
    <div class="realEstatePDF">
        <div class="mod-1">
            <div class="inner clearfix">
                <div class="hd">The 5 closest schools to the property you are considering.</div>
                <div class="bd">
                    <p class="">These results are provided only as a guide to prospective home buyers. Please check with
                        your school district for more detailed enrollment information or visit www.greatschools.org.</p>
                    <div class="tableHeader clearfix">
                        <!--<div class="fl" style="width: 220px">hello&lt;!&ndash;do not collapse&ndash;&gt;</div>-->
                        <div class="fl tac smallBold" style="padding-left:220px; width: 140px;">Distance</div>
                        <div class="fl tac smallBold" style="width: 193px">GreatSchools rating</div>
                        <div class="fr tac smallBold" style="width: 135px;">Community rating</div>
                    </div>
                    <c:forEach items="${schoolSearchResults}" var="school" varStatus="status">
                        <realEstate:schoolReportSection school="${school}" schoolMediaElement="${schoolMediaList[0]}"/>
                        <!--<hr class="mvm">&lt;!&ndash; do not collapse &ndash;&gt;</hr>-->
                    </c:forEach>
                </div>
            </div>
        </div>
        <div class="realEstate-pb" />
        <div class="realEstate-profile mod-3">
            <div class="inner">
                <div class="realEstateProfile clearfix">
                    <div class="fl">
                        <c:choose>
                            <c:when test="${photoMediaPath != null}">
                                <img class="js-logo-pending"
                                     src="${basePhotoPath}${photoMediaPath}"
                                     width="145" height="145"
                                     alt="photo"/>
                            </c:when>
                            <c:otherwise>
                                <img class="js-logo-pending"
                                     src="/res/img/realEstate/upload_placeholder.png"
                                     width="145" height="145"
                                     alt="photo"/>
                            </c:otherwise>
                        </c:choose>
                    </div>
                    <div class="fl profileData" style="vertical-align: middle">
                        <div class="largeBold make-555555">${agentFirstName} ${agentLastName}</div>
                        <div class="default make-555555">${agentCompany}</div>
                        <div class="default make-555555"><span class="defaultBold">office:</span> ${agentWorkNumber}</div>
                        <c:if test="${not empty agentCellNumber}">
                            <div class="default make-555555"><span class="defaultBold">mobile:</span> ${agentCellNumber}</div>
                        </c:if>

                        <div class="default make-555555">${agentEmail}</div>
                    </div>
                    <div class="companyLogo fr">
                        <c:choose>
                            <c:when test="${logoMediaPath != null}">
                                <img class="js-logo-pending"
                                     src="${basePhotoPath}${logoMediaPath}"
                                     width="100" height="100"
                                     alt="photo"/>
                            </c:when>
                            <c:otherwise>
                                <img class="js-logo-pending"
                                     src="/res/img/realEstate/upload_placeholder.png"
                                     width="100" height="100"
                                     alt="photo"/>
                            </c:otherwise>
                        </c:choose>
                    </div>
                </div>
                <div class="qrCode clearfix">
                    <div class="fl">
                        <div class="code">
                            <![CDATA[<img src="${imgSrc}?content=${locationSearchUrl}&amp;width=145&amp;height=145&amp;isUrl=true" alt="Unable to generate QR code. Please try again."/>]]>
                        </div>
                    </div>
                    <div class="fl qrCodeDescription make-555555 default" style="width: 485px;">
                        For more information on these schools, please scan this QR code for access to your personalized GreatSchools
                        School Search landing page. There you will find in-depth information on these schools including test scores,
                        demographics, community ratings and much more.
                    </div>
                </div>
            </div>
        </div>
    </div>
    </body>
    </html>
</jsp:root>
