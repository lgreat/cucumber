<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:mod="urn:jsptagdir:/WEB-INF/tags/mod"
          version="2.0">

    <html>
    <jsp:directive.page contentType="text/html"/>

    <head>
        <title>Create Your Neighborhood School Guide | GreatSchools</title>
        <meta name="decorator" content="standardDecorator-v1"/>
        <meta name="robots" content="noindex"/>
        <gsml:javascript src="http://maps.google.com/maps/api/js?sensor=false&amp;libraries=geometry"/>
        <gsml:javascript src="/res/js/realEstateAgent/createReport.js"/>
    </head>

    <body>
    <div class="imageBgScale">
        <div class="pr" style="height:200px;">
            <div class="headerBar" style="">
                <p style="font-family: 'SourceSansPro-Semibold' arial; font-size:48px; color: #FFFFFF; padding: 40px 0 20px 0;text-shadow: black 0.1em 0.1em 0.2em">Get your personalized printout</p>
                <p style="font-family: 'SourceSansPro-BoldIt' arial; font-size:16px; font-style: italic; color: #b2811d; padding-bottom: 60px;">Fill the forms out to finish your personalized GreatSchool printout</p>
            </div>

        </div>
        <div class="container_24 clearfix">
            <div class="pa" style="top:135px;">
                <img src="/res/img/realEstate/school_guide_960x230.png" alt="School guide image"/>
            </div>
        </div>

        <div class="container_24 clearfix" style="margin-top: 205px;">
            <div class="prefix_3 grid_18 suffix_3">
                <div class="">
                    <div class="">
                        <div class="">
                            <form action='/real-estate/guides/neighborhood-guide.pdf' METHOD='POST' target="_blank" id="jq-propertyDetailsForm">
                                <div class="mbl jq-addressFields">
                                    <gsml:input type="text" styleId="jq-address" styleClass="text1 input_18" name="address"
                                                value="Property Address" onfocus="GS_textSwitch(this, '', 'Property Address');" onblur="GS_textSwitch(this, 'Property Address', '');"/>

                                    <div class="errors">
                                        <span class="error valid">&amp;nbsp;</span>
                                        <span class="error invalid"><!-- not empty --></span>
                                    </div>
                                </div>

                                <div class="line">
                                    <div class="unit size34of100">
                                        <select id="js-bed" class="" name="bed">
                                            <gsml:selectOption value="" selected="true">Bedrooms</gsml:selectOption>
                                            <gsml:selectOption value="1">1</gsml:selectOption>
                                            <gsml:selectOption value="2">2</gsml:selectOption>
                                            <gsml:selectOption value="3">3</gsml:selectOption>
                                            <gsml:selectOption value="4">4</gsml:selectOption>
                                            <gsml:selectOption value="5">5</gsml:selectOption>
                                        </select>
                                    </div>

                                    <div class="unit size34of100">
                                        <select id="js-bath" class="" name="bath">
                                            <gsml:selectOption value="" selected="true">Bathrooms</gsml:selectOption>
                                            <gsml:selectOption value="1">1</gsml:selectOption>
                                            <gsml:selectOption value="2">2</gsml:selectOption>
                                            <gsml:selectOption value="3">3</gsml:selectOption>
                                            <gsml:selectOption value="4">4</gsml:selectOption>
                                            <gsml:selectOption value="5">5</gsml:selectOption>
                                        </select>
                                    </div>

                                    <div class="unit size34of100 lastUnit jq-sqFootageFields">
                                        <gsml:input type="text" styleId="js-sqFeet" styleClass="text1 input_6" name="sqFeet"
                                                    value="Square footage" onfocus="GS_textSwitch(this, '', 'Square footage');" onblur="GS_textSwitch(this, 'Square footage', '');"/>

                                        <div class="errors">
                                            <span class="error valid">&amp;nbsp;</span>
                                            <span class="error invalid"><!-- not empty --></span>
                                        </div>
                                    </div>
                                </div>

                                <input type="hidden" id="jq-lat" name="lat"/>
                                <input type="hidden" id="jq-lon" name="lon"/>
                                <input type="hidden" id="jq-state" name="state"/>
                                <input type="hidden" id="jq-streetNumber" name="streetNumber"/>
                                <input type="hidden" id="jq-streetName" name="streetName"/>
                                <input type="hidden" id="jq-city" name="city"/>
                                <input type="hidden" id="jq-zipcode" name="zipcode"/>
                                <!-- TODO: comment the skip validation -->
                                <input type="hidden" id="jq-skipUserCheck" name="skipUserCheck"/>

                                <!-- TODO: remove this when pdf is redy -->
                                <input type="hidden" id="jq-pageView" name="pageView"/>

                                <div class="fr mtl">
                                    <a href="javascript:void(0)">
                                        <button class="button-1" type="button" id="js-submit"
                                                onclick="GS.realEstateAgent.createGuide.submitSearch()">
                                            Print
                                        </button>
                                    </a>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div style="display: none;">
        <mod:tracking pageName="Radar Report Generator"
                      hier1="real-estate, GreatSchools Radar, Tools"/>
    </div>

    </body>
    </html>
</jsp:root>
