<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          version="2.0">

    <html>
    <jsp:directive.page contentType="text/html"/>

    <head>
        <title>Real Estate Agent - Create Report | GreatSchools</title>
        <meta name="decorator" content="standardDecorator-v1"/>
        <gsml:javascript src="http://maps.google.com/maps/api/js?sensor=false&amp;libraries=geometry"/>
        <gsml:javascript src="/res/js/realEstateAgent/createReport.js"/>
    </head>

    <body>
    <div class="slStylesV1-0 slFormsV1-0">
        <div class="spacer_20"><!-- do not collapse --></div>
        <div class="grid_21">
            <h1 class="fltlft">Create Report Form</h1>
        </div>

        <div class="grid_24">
            <div class="grid_24 alpha">
                <div class="mod form">
                    <div class="inner">
                        <div class="bd">
                            <form action='/real-estate/guides/neighborhood-guide.pdf' METHOD='POST' target="_blank" id="jq-propertyDetailsForm">
                                <div class="lal clearfix">
                                    <gsml:input type="text" styleId="jq-address" styleClass="text input_12" name="address"
                                                placeholder="Property Address"/>
                                </div>

                                <div class="lal line">
                                    <div class="unit fl">
                                        <select id="js-bed" class="" name="bed">
                                            <gsml:selectOption value="" selected="true">Bedrooms</gsml:selectOption>
                                            <gsml:selectOption value="1">1</gsml:selectOption>
                                            <gsml:selectOption value="2">2</gsml:selectOption>
                                            <gsml:selectOption value="3">3</gsml:selectOption>
                                            <gsml:selectOption value="4">4</gsml:selectOption>
                                            <gsml:selectOption value="5">5</gsml:selectOption>
                                        </select>
                                    </div>

                                    <div class="unit fl">
                                        <select id="js-bath" class="" name="bath">
                                            <gsml:selectOption value="" selected="true">Bathrooms</gsml:selectOption>
                                            <gsml:selectOption value="1">1</gsml:selectOption>
                                            <gsml:selectOption value="2">2</gsml:selectOption>
                                            <gsml:selectOption value="3">3</gsml:selectOption>
                                            <gsml:selectOption value="4">4</gsml:selectOption>
                                            <gsml:selectOption value="5">5</gsml:selectOption>
                                        </select>
                                    </div>

                                    <div class="lastUnit fr">
                                        <gsml:input type="text" styleId="js-sqFeet" styleClass="text input_3" name="sqFeet"
                                                    placeholder="Square footage"/>
                                    </div>
                                </div>

                                <input type="hidden" id="jq-lat" name="lat"/>
                                <input type="hidden" id="jq-lon" name="lon"/>
                                <input type="hidden" id="jq-state" name="state"/>
                                <input type="hidden" id="jq-streetNumber" name="streetNumber"/>
                                <input type="hidden" id="jq-streetName" name="streetName"/>
                                <input type="hidden" id="jq-city" name="city"/>
                                <input type="hidden" id="jq-zipcode" name="zipcode"/>

                                <a href="javascript:void(0)">
                                    <button class="button-1" type="button" id="js-submit" onclick="GS.realEstateAgent.submitSearch()">
                                        Submit
                                    </button>
                                </a>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    </body>
    </html>
</jsp:root>
