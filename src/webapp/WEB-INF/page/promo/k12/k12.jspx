    <html lang="en">
    <jsp:directive.page contentType="text/html"/>

    <ad:setSlotPrefix slotPrefix="K12_Advertiser_Page"/>

    <head>
        <title>Online Education, ${schoolName} | GreatSchools</title>
        <!-- hardcoded to www.greatschools.org instead of using hostname due to cobrand urls -->
        <link rel="canonical" href="http://www.greatschools.org/online-education.page?school=${k12School}"/>

        <pageHelper:addMetaProperty property="description" content="Online education for grades K-12, providing individualized learning customized to each child's needs with award winning curriculum that engages young minds."/>
        <pageHelper:addMetaProperty property="keywords" content="online education,online resources,K-12 educational online resources,virtual academies"/>

        <meta name="decorator" content="standardDecorator-v1"/>

        <script type="text/javascript">
            jQuery(function() {
                jQuery('#k12AdPage .jq-k12ClickThrough').
                        unbind('click').click(function() {
                            if (s.tl) {
                                s.tl(true, 'o', 'K12_ClickThrough_${k12School}');
                            }
                            return true;
                        });
            });
        </script>



    </head>

    <gsml:body adPageName="k12AdvertiserPage" pageLayout="nocol">

    <!--define state specific variables tagx-->
    <k12:defineStateSpecificVariables school="${k12School}"/>
    <c:set var="learnMoreHereButton">
        <a href="${k12ClickThroughUrl}" class="but but-rg but-1 jq-k12ClickThrough">Learn More &amp;raquo;</a>
    </c:set>

