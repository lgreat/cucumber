                    </c:forEach>
                </div>
            </div>

            <!--want to learn more small ad-->
            <div class="grid_5 inner fr">
                <div class="small_bold bottom pal">
                    <h5>
                    Want to learn more about K12 International?
                    </h5>
                </div>
                <div class="">
                    ${learnMoreHereButton}
                </div>

