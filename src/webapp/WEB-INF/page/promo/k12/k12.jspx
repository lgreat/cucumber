    <div id="k12AdPage">
        <div class="grid_16 mod standard_1-1">
            <div class="line">
                <div class="mtl">
                    <h1 class=""><c:if test="${hasLogo}"><img src="/res/img/promo/k12/${schoolCode}.png" alt="${schoolName} logo" width="100" height="25"/></c:if>${schoolName}</h1>
                </div>
            </div>
            <div>
                Online
                |
                K-12
                |
                Tuition-Free Online Public School
            </div>
        </div>

        <!--k12 logo-->
        <div id="logo" class="grid_6 mll mtl">
            <div class="bd small fl">
                Sponsored<br/>by:
            </div>
            <div class="imgExt mbl fl">
                <gsml:img src="/res/img/promo/k12/k12-sponsored-by-logo.png"
                          alt="GreatSchools: Involved Parents. Successful Kids" width="50" height="38"/>
            </div>
            <div class="fl mbl">
                <strong>Website: </strong> <a href="${k12ClickThroughUrl}">www.K12.com/${fn:toUpperCase(schoolCode)}</a>
            </div>
        </div>

        <!--main text-->
        <div id="mainText" class="grid_11 mod standard_1-1">
            <div class="inner">
                <div class="hd pas">
                    <h4 class="bottom">About ${schoolName}</h4>
                </div>
                <div class="pam">
                    <k12:topCopy/>
