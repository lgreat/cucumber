<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="1.2"
          xmlns="http://www.w3c.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:mod="urn:jsptagdir:/WEB-INF/tags/mod"
          xmlns:promo="urn:jsptagdir:/WEB-INF/tags/promo"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:link="urn:www.greatschools.net/taglib/link"
          xmlns:search="urn:jsptagdir:/WEB-INF/tags/search"
          xmlns:gsweb="gstaglib"
          xmlns:display="urn:jsptld:http://displaytag.sf.net"
        >
<html lang="en">

<jsp:output doctype-root-element="html"
            doctype-public="-//W3C//DTD XHTML 1.0 Transitional//EN"
            doctype-system="http://www.w3c.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"/>

<jsp:directive.page contentType="text/html"/>
<pageHelper:externalCss file="/css/spp2.css"/>

<head>
    <title>Rating page</title>

    <pageHelper:externalCss file="/css/spp2.css"/>

    <style type="text/css">
        .contentGS td.tab a, a:active {
            color: #FFF;
        }

        .contentGS td a:hover {
            color: #000;
        }

        td span#infolink, span.moreabout {
            text-decoration: underline;
        }

        #adSkyscraper {
            padding-top: 0px;
        }

        @media print {
            .search, #adLeaderboard, #footer, .noprint  { display:none; }
            .wrap0  { width:75%; background:white; }
            body, body.unframed #pageGS {
                background-color:#fff;
                border: 0px solid   #fff;
            }
        }

        #rating table {
            border: 1px solid #000;
            border-collapse: collapse;
            font-family: arial, sans-serif;
            font-size: 80%;
            text-align: center;
            background-color: #ffffff;
        }

        #rating thead, #rating tbody {
            padding: 0 10px 0 10px;
        }

        #rating th {
            border-bottom: 1px solid #000;
            border-collapse: collapse;
            padding: 5px;
            background-color: #65CDCB;
            color: #ffffff
        }

        .vb {
            background: url( 'http://www.greatschools.net/images/carrowts2dn.gif' ) no-repeat 63% 50%;
        }

        .b {
            background: url( 'http://www.greatschools.net/images/carrowts1dn.gif' ) no-repeat 78% 50%;
        }

        .nc {
            background: url( 'http://www.greatschools.net/images/carrowts1right.gif' ) no-repeat 58% 50%;
        }

        .g {
            background: url( 'http://www.greatschools.net/images/carrowts1.gif' ) no-repeat 78% 50%;
        }

        .vg {
            background: url( 'http://www.greatschools.net/images/carrowts2.gif' ) no-repeat 63% 50%;
        }

        /*td[scope='rowgroup'] TODO remove class="rowgroup" when IE supports attribute selectors*/
        .rowgroup {
            border-bottom: 1px solid #000;
            border-collapse: collapse;
            font-weight: bold;
            text-align: left;
            background-color: #E0EDEE;
            color: #65CDCB;
        }

        /*td[scope='row'] TODO remove class="rowgroup" when IE supports attribute selectors*/
        .row {
            text-align: left;
        }

        #rating tr:hover {
            background-color: #eee;
        }

        .heading:hover {
            background-color: #fff;
        }
    </style>
</head>

<gsml:body adPageName="ratings" pageLayout="research">

    <div id="printandemail" class="noprint">
        <a href="%" onClick=" window.print(); return false">Print this page</a>&amp;nbsp;&amp;nbsp;&amp;nbsp;|&amp;nbsp;&amp;nbsp;&amp;nbsp;<a
            href="/cgi-bin/mail_to_friend/authorizer/${requestScope.context.state.abbreviationLowerCase}/${school.id}"
            onClick=" window.open('/cgi-bin/mail_to_friend/authorizer/${requestScope.context.state.abbreviationLowerCase}/${school.id}', 'win', 'width=600,height=550,scrollbars=yes'); return false">Email
        to a friend</a>
    </div>

    <div class="wrap0">
        <div class="wrap1">
            <div class="wrap2">
                <div class="wrap3">
                    <div class="box">
                        <table cellspacing="0" cellpadding="0" width="100%" border="0">
                            <tr>
                                <td colspan="4">
                                    <table class="rollmenu" cellspacing="0" cellpadding="0">
                                        <tr>
                                            <td class="tab"><gsml:a href="/modperl/browse_school/$STATE/${school.id}">
                                                School<br/>
                                                Overview</gsml:a></td>
                                            <td class="tab"><gsml:a href="/modperl/achievement/$STATE/${school.id}">
                                                Test<br/>Scores
                                            </gsml:a></td>
                                            <td class="tab"><gsml:a href="/cgi-bin/$LCSTATE/other/${school.id}">
                                                Teachers/<br/>
                                                Students</gsml:a></td>
                                            <c:if test="${requestScope.context.state.ratingsState}">
                                                <td class="tab"><gsml:a href="/cgi-bin/$LCSTATE/rankings/${school.id}">
                                                    Ratings and
                                                    <br/> Alerts</gsml:a></td>
                                            </c:if>
                                            <td class="tab"><gsml:a href="/modperl/parents/$STATE/${school.id}">Parent
                                                <br/> Reviews
                                            </gsml:a></td>
                                            <td class="tab"><gsml:a href="/cgi-bin/$LCSTATE/pqview/${school.id}">
                                                Principal's <br/>
                                                View</gsml:a></td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </table>

                        <gsweb:ratingTable columns="${columns}" rowGroups="${rowGroups}"/>
                    </div>
                </div>
            </div>
        </div>
    </div>


</gsml:body>

</html>
</jsp:root>