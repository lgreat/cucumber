<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="1.2"
          xmlns="http://www.w3c.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:mod="urn:jsptagdir:/WEB-INF/tags/mod"
          xmlns:promo="urn:jsptagdir:/WEB-INF/tags/promo"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:link="urn:www.greatschools.net/taglib/link"
          xmlns:search="urn:jsptagdir:/WEB-INF/tags/search"
          xmlns:gsweb="gstaglib"
          xmlns:display="urn:jsptld:http://displaytag.sf.net"
          xmlns:ad="urn:jsptagdir:/WEB-INF/tags/ad"
          xmlns:school="urn:jsptagdir:/WEB-INF/tags/school"
        >
<html lang="en">

<jsp:output doctype-root-element="html"
            doctype-public="-//W3C//DTD XHTML 1.0 Transitional//EN"
            doctype-system="http://www.w3c.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"/>

<jsp:directive.page contentType="text/html"/>

<jsp:scriptlet>
    gs.web.util.PageHelper.setAdServerControlledFooterAd(request, true);
</jsp:scriptlet>


<head>
    <title>Rating page</title>

    <style type="text/css">
        #rating {
            border-left: 2px solid #deeeee;
            border-right: 2px solid #deeeee;
            border-bottom: 2px solid #deeeee;
            border-collapse: collapse;
            font-family: arial, sans-serif;
            background-color: #ffffff;
            width:100%;
            color:#666;
            font-weight:100;
        }

        #rating th {
            padding: 5px 0 5px 40px;
            border-collapse: collapse;
            background-color: #66cccc;
            color: #ffffff;
            width:50%;
        }

        #rating td {
            padding: 5px 0 5px 40px;
        }

        #rating td.rowgroup {
            padding: 20px 0 0 0;
            font-weight: bold;
            color: #55bbcc;

        }

        #rating td.rowgroup span {
            margin: 0 10px 0 10px;
            padding: 0 30px 0 30px;
            background-color: #deeeee;
            display:block;
        }


        .vb {
            background: url( 'http://www.greatschools.net/images/carrowts2dn.gif' ) no-repeat;
            text-indent:-5000px;
            width: 10px;
            height: 30px;
        }

        .b {
            background: url( 'http://www.greatschools.net/images/carrowts1dn.gif' ) no-repeat 78% 50%;
        }

        .nc {
            background: url( 'http://www.greatschools.net/images/carrowts1right.gif' ) no-repeat 58% 50%;
        }

        .g {
            background: url( 'http://www.greatschools.net/images/carrowts1.gif' ) no-repeat 78% 50%;
        }

        .vg {
            background: url( 'http://www.greatschools.net/images/carrowts2.gif' ) no-repeat 63% 50%;
        }

    </style>
</head>


<pageHelper:externalCss file="/res/css/school/ratings.css"/>
<gsml:body adPageName="ratings" pageLayout="research">

<div class="marginGS" id="marginGS">
    <school:profile school="${school}">
        <div class="ratingSummaryWrapper">
            <div class="ratingSummaryContent">
                <gsml:img src="/res/img/school/ratings/ratings_01.gif" alt="ratings" style="float:left;padding-right:20px;"/>
                <h2>What do these ratings mean?</h2>

                <p>These ratings provide a quick overview of how students<br/>
                    in this school performed on standardized tests.
                </p>
                <br/>
                <p>
                    A "10" means that the group is in the top 10% for test scores<br/>
                    when compared to students in the same group statewide.<br/>
                    A "1" means that the group is in the bottom 10%.
                </p>
            </div>
        </div>

        <div style="padding-top:15px;">

            <div style="float:left;width:498px;">
                <!--<gsweb:ratingTable columns="${columns}" rowGroups="${rowGroups}"/>-->
<table id="rating">
  <thead>
    <tr>
      <th scope="col">STUDENTS</th>
      <th scope="col">GREATSCHOOLS<br/><span style="padding-left:30px;">RATING</span></th>
    </tr>
  </thead>

    <tbody>
      <tr>
        <td scope="row">All Students</td>
        <td>Combined score</td>
      </tr>
    </tbody>

  <tbody>
    <tr>
      <td colspan="2" scope="rowgroup" class="rowgroup"><span>By Gender</span></td>
    </tr>
    <tr>
      <td scope="row">Male</td>
      <td>Combined score</td>
    </tr>

    <tr>
      <td scope="row">Female</td>
      <td>Combined score</td>
    </tr>
  </tbody>

  <tbody>
    <tr>
      <td colspan="2" scope="rowgroup" class="rowgroup"><span>By Category</span></td>
    </tr>

    <tr>
      <td scope="row">Rich</td>
      <td><span class="vb">a</span>Combined score</td>
    </tr>

    <tr>
      <td scope="row">Poor</td>
      <td>Combined score</td>
    </tr>
  </tbody>

  <tbody>
    <tr>
      <td colspan="2" scope="rowgroup" class="rowgroup"><span>By Ethnicity</span></td>
    </tr>

    <tr>
      <td scope="row">Black</td>
      <td>Combined score</td>
    </tr>

    <tr>
      <td scope="row">White</td>
      <td>Combined score</td>
    </tr>
  </tbody>
</table>
                <br/>

                <div id="legendWrapper">
                    <div id="legend">
                        <div style="float:left;">
                            <h2 id="testScoreTrend">Test Score Trend</h2>
                            <p>Based on changed between<br/>spring 2005 and spring 2006</p>
                        </div>
                        <div style="float:left;border-left: dotted 1px #66cccc;padding-left:36px; height:100%;">
                            <ul>
                                <li>Strong improvement</li>
                                <li>Some improvement</li>
                                <li>No change</li>
                                <li>Some decline</li>
                                <li>Strong decline</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>

            <div style="float:right;">
                <ad:x48/>

                <div id="moreResources">
                    <h2>More Resources</h2>
                    <ul>
                        <li>More About GreatSchools Ratings</li>
                        <li>Compare with GreatSchools Ratings<br/> from nearby schools</li>
                        <li>See this school's performance data</li>
                    </ul>
                </div>
            </div>
        </div>
   </school:profile>
</div>
</gsml:body>
</html>
</jsp:root>