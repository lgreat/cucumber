                    <form:errors path="submitterEmail" />
                    <br/>

                    <form:label path="districtInfoFields.districtName">District name: </form:label>
                    <form:input path="districtInfoFields.districtName"/>
                    <form:errors path="districtInfoFields.districtName" />
                    <br/>

                    <form:label path="state" for="districtInfoFields.state">State: </form:label>
                    <form:select path="state" id="districtInfoFields.state">
                        <form:options items="${allStateOptions}" itemValue="value" itemLabel="name"/>
                    </form:select>
                    <form:errors path="state" />
                    <br/>

                    <form:label path="districtInfoFields.referenceUrl">Reference url: </form:label>
                    <form:input path="districtInfoFields.referenceUrl"/>
                    <form:errors path="districtInfoFields.referenceUrl" />
                    <br/>

                    <form:label path="districtInfoFields.relationship">Relationship to school: </form:label>
                    <form:input path="districtInfoFields.relationship"/>
                    <form:errors path="districtInfoFields.relationship" />
                    <br/>

                    <form:label path="districtInfoFields.comment">Question/comment: </form:label>
                    <form:textarea path="districtInfoFields.comment" rows="3" cols="20"/>
                    <form:errors path="districtInfoFields.comment" />
                    <br/>

                    <div class="captcha" id="incorrectDistrict_captcha">
                        <!-- no collapse -->
                    </div>
                    <form:errors path="response" />
                    <br/>

                    <input type="submit" name="Submit" value="submit"/>
                </form:form>
            </div>
