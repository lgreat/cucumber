<?xml version="1.0" encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="1.2"
          xmlns="http://www.w3c.org/1999/xhtml"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:fmt="http://java.sun.com/jsp/jstl/fmt"
          xmlns:spring="http://www.springframework.org/tags"
          xmlns:gsml="urn:jsptagdir:/WEB-INF/tags/gsml"
          xmlns:ad="urn:jsptagdir:/WEB-INF/tags/ad"
          xmlns:mod="urn:jsptagdir:/WEB-INF/tags/mod"
          xmlns:pageHelper="urn:jsptagdir:/WEB-INF/tags/pageHelper"
          xmlns:about="urn:jsptagdir:/WEB-INF/tags/about"
          xmlns:link="urn:www.greatschools.net/taglib/link"
          xmlns:gsweb="gstaglib"
        >
<html lang="en">
<jsp:output doctype-root-element="html"
            doctype-public="-//W3C//DTD XHTML 1.0 Transitional//EN"
            doctype-system="http://www.w3c.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"/>

<jsp:directive.page contentType="text/html"/>
<pageHelper:setPathway pathway="HOME"/>
<pageHelper:hideFooterAd/>

<head>
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8"/>
    <meta name="Description" content="GreatSchools in the News"/>
    <title>GreatSchools in the News</title>
    <pageHelper:externalCss file="/res/css/widelayout.css"/>
    <pageHelper:externalCss file="/res/css/about/pressRoom/pressClippings.css"/>
</head>

<ad:setSlotPrefix slotPrefix="Press_Clippings"/>

<gsml:body adPageName="pressClippings" pageLayout="pressClippings">

    <div class="pageTitle">
        <h2>GreatSchools in the News</h2>
    </div>

    <div class="description">
        GreatSchools has received coverage in major national, state and local newspapers, as well as radio
        and television broadcasts. Below are some highlights. Click on any title for the complete story.
        Note that you may have to register for online news stories.
    </div>

    <div class="content">
        <jsp:include page="/about/pressClippings.module"/>
    </div>

    <div class="contact">
        <h3>Contact GreatSchools</h3>
        David Steer<br/>
        415.977.0700 x 123<br/>
        <a href="mailto:pr@greatschools.net">pr@greatschools.net</a>
    </div>

    <div style="display: none;">
        <mod:tracking pageName="Press Clippings" hier1="Home,Corporate,AboutUs,PressRoom,PressClippings"/>
    </div>

</gsml:body>
</html>
</jsp:root>