<beans>
    <bean name="xxxdefaultHandlerMapping"
          class="org.springframework.web.servlet.handler.BeanNameUrlHandlerMapping">
        <property name="interceptors">
            <list>
                <bean class="gs.web.util.OpenSessionInViewInterceptor"/>
                <!--ref bean="requiresStateInterceptor"/-->
                <bean class="gs.web.ResponseInterceptor"
                      autowire="byName"/>
                <bean class="gs.web.util.context.SessionContextInterceptor"
                      autowire="byName">
                </bean>
            </list>
        </property>
        <property name="alwaysUseFullPath" value="true"/>
    </bean>

    <bean name="defaultHandlerMapping"
          class="org.springframework.web.servlet.handler.SimpleUrlHandlerMapping">
        <property name="interceptors">
            <list>
                <bean class="gs.web.util.OpenSessionInViewInterceptor"/>
                <!--ref bean="requiresStateInterceptor"/-->
                <bean class="gs.web.ResponseInterceptor"
                      autowire="byName"/>
                <bean class="gs.web.util.context.SessionContextInterceptor"
                      autowire="byName">
                </bean>
            </list>
        </property>
        <property name="urlMap">
            <map>
                <entry key="/city/**" value="/city.page"></entry>
            </map>
        </property>
        <property name="alwaysUseFullPath" value="true"/>
    </bean>


    <bean id="requiresStateInterceptor"
          class="gs.web.state.RequiresStateInterceptor"
          autowire="byName">
    </bean>

